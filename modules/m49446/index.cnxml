    <m:mn>12</m:mn>
   </m:mrow>
  </m:msub>
  <m:mo>.</m:mo>
 </m:mrow>
</m:math> </para></problem>
</exercise>

<exercise id="fs-id1165137727503">
<problem id="fs-id1165135191604">
<para id="fs-id1165135187574"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mfrac>
    <m:mn>2</m:mn>
    <m:mn>3</m:mn>
   </m:mfrac>
   <m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
    <m:mn>2</m:mn>
    <m:mn>9</m:mn>
   </m:mfrac>
   <m:mo>,</m:mo><m:mfrac>
    <m:mn>2</m:mn>
    <m:mrow>
     <m:mn>27</m:mn>
    </m:mrow>
   </m:mfrac>
   <m:mo>,</m:mo><m:mn>...</m:mn>
  </m:mrow> <m:mo>}</m:mo></m:mrow><m:mo>.</m:mo>
 </m:mrow>
</m:math> Find <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
