   <exercise id="fs-id1165135526045">
    <problem id="fs-id1165135526047">
     <para id="fs-id1165135526049"> <m:math>
      <m:mrow>
       <m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mi>sin</m:mi></m:mrow>
          <m:mrow>
           <m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
         </m:msup>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mi>x</m:mi><m:mo>+</m:mo><m:mfrac>
            <m:mn>1</m:mn>
            <m:mn>2</m:mn>
           </m:mfrac>
          </m:mrow>
          <m:mo>)</m:mo></m:mrow></m:mrow>
        <m:mo>)</m:mo></m:mrow></m:mrow>
     </m:math>
     </para>
    </problem>
    <solution id="fs-id1165134081072">
     <para id="fs-id1165134081074"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>0.5</m:mn></m:mrow>
        <m:mrow>
         <m:msqrt>
          <m:mrow>
           <m:mo>−</m:mo><m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mo>−</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mfrac>
            <m:mn>3</m:mn>
            <m:mn>4</m:mn>
           </m:mfrac>
          </m:mrow>
         </m:msqrt>
        </m:mrow>
       </m:mfrac>
      </m:mrow>
     </m:math>
     </para>
    </solution>
   </exercise>

