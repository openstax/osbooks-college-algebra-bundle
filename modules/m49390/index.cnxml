         </m:msup>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mn>1</m:mn><m:mo>−</m:mo><m:mi>x</m:mi></m:mrow>
          <m:mo>)</m:mo></m:mrow></m:mrow>
        <m:mo>)</m:mo></m:mrow></m:mrow>
     </m:math>
     </para>
    </problem>
   </exercise>
   <exercise id="fs-id1165135252123">
    <problem id="fs-id1165135252125">
     <para id="fs-id1165135252127"> <m:math>
      <m:mrow>
       <m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mi>sin</m:mi></m:mrow>
          <m:mrow>
           <m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
         </m:msup>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mfrac>
            <m:mn>1</m:mn>
