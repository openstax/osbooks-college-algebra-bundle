        <m:mn>5</m:mn><m:mi>π</m:mi>
       </m:mrow>
       <m:mn>6</m:mn>
      </m:mfrac>

     </m:mrow>
    <m:mo>)</m:mo></m:mrow>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>5</m:mn><m:mi>π</m:mi>
   </m:mrow>
   <m:mn>6</m:mn>
  </m:mfrac>

 </m:mrow>
