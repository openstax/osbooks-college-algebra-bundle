  </m:mfrac>
  <m:mo>;</m:mo>
 </m:mrow>
</m:math> midline: <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo>
 </m:mrow>
</m:math>  asymptotes: <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mo>=</m:mo><m:mfrac>
   <m:mi>π</m:mi>
   <m:mn>3</m:mn>
  </m:mfrac>
