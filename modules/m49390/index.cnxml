         </m:msup>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mn>3</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
          <m:mo>)</m:mo></m:mrow></m:mrow>
