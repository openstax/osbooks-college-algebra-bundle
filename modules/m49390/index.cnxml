          </m:mrow>
          <m:mo>)</m:mo></m:mrow></m:mrow>
        <m:mo>)</m:mo></m:mrow></m:mrow>
     </m:math>
     </para>
    </problem>
    <solution id="fs-id1165135252184">
     <para id="fs-id1165135252186"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msqrt>
          <m:mrow>
           <m:msup>
            <m:mi>x</m:mi>
            <m:mn>2</m:mn>
           </m:msup>
           <m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
         </m:msqrt>
        </m:mrow>
        <m:mi>x</m:mi>
       </m:mfrac>
      </m:mrow>
     </m:math>
     </para>
    </solution>
   </exercise>
   <exercise id="fs-id1165135525984">
    <problem id="fs-id1165135525986">
     <para id="fs-id1165135525988"> <m:math>
