   <m:mrow>
    <m:mn>3</m:mn><m:mi>x</m:mi>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mn>4</m:mn><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
