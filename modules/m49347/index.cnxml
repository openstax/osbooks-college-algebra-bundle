 <m:mrow>
  <m:mn>0</m:mn></m:mrow></m:math>
 with multiplicity <m:math><m:mrow><m:mn>4</m:mn><m:mtext>,</m:mtext><m:mspace width="0.8em"/><m:mn>2</m:mn></m:mrow></m:math>

