  </media>
  </solution>
  </exercise><exercise id="fs-id1165137748677">
  <problem id="fs-id1165137748679">
  <para id="fs-id1165137748681"> <m:math>
  <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
