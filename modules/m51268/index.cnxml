  <exercise id="fs-id1165135517182">
  <problem id="fs-id1165135517184">
  <para id="fs-id1165135517186">Find <m:math>
  <m:mrow>
 <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo></m:mrow>
  </m:math>
  </para>
  </problem>
  <solution id="fs-id1165137762681">
  <para id="fs-id1165137762683"> <m:math>
  <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn></m:mrow>
  </m:math>
  </para>
  </solution>
  </exercise>

  <exercise id="fs-id1165137834540">
  <problem id="fs-id1165137443779">
  <para id="fs-id1165137443781">Find <m:math>
   <m:mrow>
 <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>−2</m:mn><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo>
   </m:mrow>
  </m:math>
  </para></problem>
  </exercise>

  <exercise id="fs-id1165135168371">
  <problem id="fs-id1165135168373">
  <para id="fs-id1165137556971">Write an equation for the piecewise function.</para>
  </problem>
  <solution id="fs-id1165137556975">
  <para id="fs-id1165137726425"> <m:math>
  <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
  <m:mtable>
  <m:mtr>
  <m:mtd>
  <m:mrow>
  <m:mrow><m:mo>|</m:mo> <m:mi>x</m:mi> <m:mo>|</m:mo></m:mrow><m:mspace width="0.5em"/><m:mspace width="0.5em"/><m:mspace width="0.5em"/><m:mtext>if</m:mtext><m:mspace width="0.5em"/><m:mspace width="0.5em"/><m:mi>x</m:mi><m:mo>≤</m:mo><m:mn>2</m:mn></m:mrow>
  </m:mtd>
  </m:mtr>
  <m:mtr>
  <m:mtd>
  <m:mrow>
  <m:mn>3</m:mn><m:mspace width="0.5em"/><m:mspace width="0.5em"/><m:mspace width="0.5em"/><m:mspace width="0.5em"/><m:mspace width="0.5em"/><m:mtext>if</m:mtext><m:mspace width="0.5em"/><m:mspace width="0.5em"/><m:mi>x</m:mi><m:mo>></m:mo><m:mn>2</m:mn></m:mrow>
  </m:mtd>
  </m:mtr>

  </m:mtable></m:mrow> </m:mrow></m:mrow>
  </m:math>
  </para>
  </solution>
  </exercise>

