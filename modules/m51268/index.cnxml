  <exercise id="fs-id1165137653012" class="material-set-2"><problem id="fs-id1165137427968">

  <media id="fs-id1165135168455" alt="Graph of a relation." display="block">
  <image mime-type="image/jpg" src="../../media/CNX_Precalc_Figure_01_07_211.jpg"/>
