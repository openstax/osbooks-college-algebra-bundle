  </problem>


  <solution id="fs-id1165137911127"><para id="fs-id1165137911128"> increasing <m:math>
  <m:mrow>
 <m:mrow><m:mo>(</m:mo>
  <m:mrow>
  <m:mn>2</m:mn><m:mo>,</m:mo><m:mi>∞</m:mi></m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo></m:mrow>
