  <m:mi>x</m:mi>
  <m:mn>5</m:mn>
  </m:msup>
  </m:mrow>
  </m:math>
  </para>
  </problem>
  </exercise>
  <exercise id="fs-id1165137770368">
  <problem id="fs-id1165135536626">
  <para id="fs-id1165135536628"> <m:math>
  <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
