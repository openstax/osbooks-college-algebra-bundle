   <m:mrow>
    <m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>∞</m:mi><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>1.1</m:mn><m:mo stretchy="false">)</m:mo><m:mspace width="0.5em"/><m:mtext>and </m:mtext><m:mo stretchy="false">(</m:mo><m:mn>1.1</m:mn><m:mo>,</m:mo><m:mi>∞</m:mi><m:mo stretchy="false">)</m:mo>
