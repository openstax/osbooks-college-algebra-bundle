          <entry align="center"><emphasis>Reciprocal squared</emphasis></entry>
          <entry align="center"><emphasis>Cube root</emphasis></entry>
          <entry align="center"><emphasis>Square root</emphasis></entry>
          <entry align="center"><emphasis>Absolute value</emphasis></entry>
          <entry/>
      </row>
      <row>

          <entry align="center"> <m:math>
              <m:mrow>
                  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
                      <m:mn>1</m:mn>
                      <m:mrow>
                          <m:msup>
                              <m:mi>x</m:mi>
