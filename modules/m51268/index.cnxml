  <exercise id="fs-id1165137447422">
  <problem id="fs-id1165137447424">
  <para id="fs-id1165137469729">Find <m:math>
  <m:mrow>
 <m:msup>
  <m:mi>F</m:mi>
  <m:mrow>
  <m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
  </m:msup>
  <m:mo stretchy="false">(</m:mo><m:mn>15</m:mn><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo></m:mrow>
