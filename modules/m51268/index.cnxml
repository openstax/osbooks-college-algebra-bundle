  <para id="fs-id1165135245866">For the following exercises, determine whether the functions are even, odd, or neither.</para>

  <exercise id="fs-id1165137451069">
  <problem id="fs-id1165137451071">
  <para id="fs-id1165137423842"> <m:math>
  <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
  <m:mn>5</m:mn>
  <m:mrow>
  <m:msup>
  <m:mi>x</m:mi>
  <m:mn>2</m:mn>
  </m:msup>
  </m:mrow>
  </m:mfrac>
  <m:mo>+</m:mo><m:mn>9</m:mn><m:msup>
  <m:mi>x</m:mi>
  <m:mn>6</m:mn>
  </m:msup>
  </m:mrow>
  </m:math>
  </para>
  </problem>
  <solution id="fs-id1165137610712">
  <para id="fs-id1165137610714"> <m:math>
  <m:mrow>
  <m:mtext>even</m:mtext></m:mrow>
  </m:math>
  </para>
  </solution>
  </exercise>
  <exercise id="fs-id1165137542465">
  <problem id="fs-id1165134250812">
  <para id="fs-id1165134250814"> <m:math>
  <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
  <m:mn>5</m:mn>
  <m:mrow>
  <m:msup>
  <m:mi>x</m:mi>
  <m:mn>3</m:mn>
  </m:msup>
  </m:mrow>
  </m:mfrac>
