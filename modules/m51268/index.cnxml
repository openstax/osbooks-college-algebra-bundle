  <para id="fs-id1165137552663"> <m:math>
  <m:mrow>
  <m:mrow><m:mo>(</m:mo>
  <m:mrow>
  <m:mo>−</m:mo><m:mi>∞</m:mi><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>2</m:mn></m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>∪</m:mo><m:mrow><m:mo>(</m:mo>
  <m:mrow>
