  <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mn>4</m:mn><m:mo stretchy="false">)</m:mo></m:mrow>
  <m:mrow>
  <m:mo>−</m:mo><m:mn>3</m:mn></m:mrow>
  </m:msup>
  </m:mrow>
  </m:mfrac>
  </m:mrow>
