  </problem>
  <solution id="fs-id1165137784950">
  <para id="fs-id1165137784952"> function </para>
  </solution>
  </exercise>
  <exercise id="fs-id1165135541845" class="material-set-2"><problem id="fs-id1165135541847">

  <media id="fs-id1165137612052" alt="Graph of a relation." display="block">
  <image mime-type="image/jpg" src="../../media/CNX_Precalc_Figure_01_07_210.jpg"/>
