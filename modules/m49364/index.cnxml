        <entry align="center">0</entry>
        <entry align="center">1</entry>
        <entry align="center">2</entry>
        <entry align="center">3</entry>
    </row>
<row>
        <entry align="center"><emphasis> <m:math>
<m:mrow>
<m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
</m:mrow>
</m:math> </emphasis></entry>
        <entry align="center">1</entry>
        <entry align="center">0</entry>
