</m:msub>
<m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mn>15</m:mn><m:mo>−</m:mo><m:mn>5</m:mn><m:mi>x</m:mi>
</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mn>6</m:mn>
</m:mrow>
