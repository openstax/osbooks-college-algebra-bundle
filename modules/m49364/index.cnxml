<para id="fs-id1165137942457"> <m:math>
<m:mrow>
<m:mi>h</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>3</m:mn><m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
<m:mi>x</m:mi>
<m:mo>)</m:mo></m:mrow><m:mo>−</m:mo><m:mn>9</m:mn>
</m:mrow>
</m:math> </para>
