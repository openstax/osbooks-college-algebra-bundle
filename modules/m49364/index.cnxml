<m:mn>2</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mn>5</m:mn>
</m:mrow>
</m:math> </para>
</problem>
</exercise>

<exercise id="fs-id1165135176226">
<problem id="fs-id1165135176229">
<para id="fs-id1165135176231"> <m:math>
<m:mrow>
<m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>x</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
</m:mrow>
<m:mo>)</m:mo></m:mrow>
</m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165135337765">
<para id="fs-id1165135181772"> <m:math>
<m:mrow>
<m:mi>x</m:mi><m:mo>≈</m:mo><m:mtext>2</m:mtext><m:mtext>.303</m:mtext>
</m:mrow>
</m:math> </para>
</solution>
</exercise>

<exercise id="fs-id1165135181791">
<problem id="fs-id1165135181793">
<para id="fs-id1165135181795"> <m:math>
<m:mrow>
