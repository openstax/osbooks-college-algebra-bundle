<exercise id="fs-id1165135367864">
<problem id="fs-id1165135367866">
<para id="fs-id1165135367869"> <m:math>
<m:mrow>
<m:mfrac>
<m:mn>1</m:mn>
<m:mn>3</m:mn>
</m:mfrac>
<m:mi>log</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mn>1</m:mn><m:mo>−</m:mo><m:mi>x</m:mi>
</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>log</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
</m:mrow>
