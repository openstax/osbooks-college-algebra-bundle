    </m:msqrt>
    <m:mo>+</m:mo><m:mn>4</m:mn><m:msqrt>
     <m:mi>a</m:mi>
    </m:msqrt>

   </m:mrow>
  </m:mfrac>
  <m:mo>.</m:mo>
 </m:mrow>
</m:math> </para>
