<para id="fs-id1165134064756"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4</m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mn>7</m:mn>
 </m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165134064796">
<para id="fs-id1165134064798"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>f</m:mi>
   <m:mo>'</m:mo>
  </m:msup>
  <m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>8</m:mn><m:mi>x</m:mi>
 </m:mrow>
</m:math> </para>
</solution>
</exercise>

