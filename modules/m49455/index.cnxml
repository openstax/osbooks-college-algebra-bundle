  </m:mfrac>

 </m:mrow>
</m:math> </para>
</problem>
</exercise>

<exercise id="fs-id1165134093704">
<problem id="fs-id1165134093706">
<para id="fs-id1165134093708"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>5</m:mn><m:msup>
