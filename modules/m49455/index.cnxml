</problem>
</exercise>

<exercise id="fs-id1165134093843">
<problem id="fs-id1165134093845">
<para id="fs-id1165134093847"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
