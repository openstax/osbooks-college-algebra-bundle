       </m:math> varies jointly as the cube of <m:math>
         <m:mrow>
          <m:mi>x</m:mi></m:mrow>
        </m:math> and as <m:math>
 <m:mrow>
  <m:mi>z</m:mi><m:mo>.</m:mo>
 </m:mrow>
</m:math> If when <m:math>
         <m:mrow>
          <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>1</m:mn></m:mrow>
        </m:math> and <m:math>
         <m:mrow>
          <m:mi>z</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mo>,</m:mo></m:mrow>
        </m:math> <m:math>
         <m:mrow>
          <m:mi>y</m:mi><m:mo>=</m:mo><m:mn>6</m:mn><m:mo>,</m:mo></m:mrow>
        </m:math> find <m:math>
         <m:mrow>
          <m:mi>y</m:mi></m:mrow>
        </m:math> if <m:math>
         <m:mrow>
          <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>2</m:mn></m:mrow>
        </m:math> and <m:math>
         <m:mrow>
          <m:mi>z</m:mi><m:mo>=</m:mo><m:mn>3.</m:mn></m:mrow>
        </m:math>
       </para></problem>
      <solution id="fs-id1165137934401">
       <para id="fs-id1165137452780"> <m:math>
        <m:mrow>
         <m:mi>y</m:mi><m:mspace width="0.5em"/><m:mtext/><m:mo>=</m:mo><m:mspace width="0.5em"/><m:mtext/><m:mn>72</m:mn></m:mrow>
