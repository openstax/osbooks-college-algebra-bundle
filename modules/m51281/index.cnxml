          <m:mn>4</m:mn>
         </m:msup>
         <m:mo>+</m:mo><m:mn>3</m:mn><m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         <m:mo>−</m:mo><m:mn>4</m:mn></m:mrow>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>2</m:mn></m:mrow>
       </m:mfrac>
      </m:mrow>
     </m:math>
     </para>
    </problem>
   <solution id="eip-idm364906096">
<para id="eip-idm364905840">
 <m:math>
<m:msup><m:mi>x</m:mi><m:mn>3</m:mn></m:msup>
<m:mo>+</m:mo>
<m:mn>2</m:mn><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup>
<m:mo>+</m:mo>
<m:mn>7</m:mn><m:mi>x</m:mi>
<m:mo>+</m:mo>
<m:mn>14</m:mn>
<m:mo>+</m:mo>
<m:mfrac>
<m:mrow>
<m:mn>26</m:mn>
</m:mrow>
<m:mrow>
<m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2</m:mn>
</m:mrow>
</m:mfrac>
</m:math>
</para>
</solution>
