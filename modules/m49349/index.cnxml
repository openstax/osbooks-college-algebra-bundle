  <section id="fs-id1165134058383">
   <title>Numeric</title>
   <para id="fs-id1165134058388">For the following exercises, list all possible rational zeros for the functions.</para>

   <exercise id="fs-id1165134058391"><problem id="fs-id1165134058392"><para id="fs-id1165134058393"> <m:math>
    <m:mrow>
     <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>
      <m:mi>x</m:mi>
      <m:mn>4</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>3</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn></m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id1165135486016"><problem id="fs-id1165135486017"><para id="fs-id1165135486018"> <m:math>
    <m:mrow>
     <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mrow>
       <m:msup>
        <m:mrow/>
        <m:mn>3</m:mn>
       </m:msup>
      </m:mrow>
     </m:msup>
     <m:mo>+</m:mo><m:mn>3</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn></m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id1165135560645"><para id="fs-id1165135560646"> <m:math>
     <m:mrow>
      <m:mo>±</m:mo><m:mn>5</m:mn><m:mo>,</m:mo><m:mspace width="0.5em"/><m:mo>±</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mspace width="0.5em"/><m:mo>±</m:mo><m:mfrac>
       <m:mn>5</m:mn>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>,</m:mo><m:mspace width="0.5em"/><m:mo>±</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>2</m:mn>
      </m:mfrac>
     </m:mrow>
    </m:math>
    </para></solution></exercise>
   <exercise id="fs-id1165134204451"><problem id="fs-id1165134204452"><para id="fs-id1165134204453"> <m:math>
    <m:mrow>
     <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>3</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mrow>
       <m:msup>
        <m:mrow/>
        <m:mn>3</m:mn>
       </m:msup>
      </m:mrow>
     </m:msup>
     <m:mo>+</m:mo><m:mn>5</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>5</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn></m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id1165132920307"><problem id="fs-id1165135179860"><para id="fs-id1165135179861"> <m:math>
    <m:mrow>
     <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>6</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>4</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>10</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>13</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id1165135575251"><para id="fs-id1165135575252"> <m:math>
     <m:mrow>
      <m:mo>±</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mspace width="0.5em"/><m:mo>±</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>,</m:mo><m:mspace width="0.5em"/><m:mo>±</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>3</m:mn>
      </m:mfrac>
      <m:mo>,</m:mo><m:mspace width="0.5em"/><m:mo>±</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>6</m:mn>
      </m:mfrac>
     </m:mrow>
    </m:math>
    </para></solution></exercise>
   <exercise id="fs-id1165135698642"><problem id="fs-id1165135698643"><para id="fs-id1165135698644"> <m:math>
    <m:mrow>
     <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4</m:mn><m:msup>
