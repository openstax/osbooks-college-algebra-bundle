   <exercise id="fs-id1165134149873" class="material-set-2"><problem id="fs-id1165134149874"><para id="fs-id1165134149875"> <m:math>
    <m:mrow>
     <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>3</m:mn>
