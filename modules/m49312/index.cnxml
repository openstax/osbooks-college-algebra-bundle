 </problem>
 <solution id="fs-id1165135560630">
  <para id="fs-id1165135560633"> <m:math>
   <m:mrow>
    <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>
     <m:mrow>
      <m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo></m:mrow>
     <m:mn>2</m:mn>
    </m:msup>
   </m:mrow>
  </m:math>
  </para></solution>
</exercise><exercise id="fs-id1165135190411"><problem id="fs-id1165135190413">

