 </problem>
 <solution id="fs-id1165134190724">
  <para id="fs-id1165134190726"> <m:math>
   <m:mrow>
    <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:msqrt>
     <m:mi>x</m:mi>
    </m:msqrt>
