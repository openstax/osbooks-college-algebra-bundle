</media>

 </problem>
<solution id="fs-id1165133030812">
 <para id="fs-id1165133030814"> <m:math>
  <m:mrow>
   <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>|</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:mo>|</m:mo><m:mo>−</m:mo><m:mn>2</m:mn></m:mrow>
 </m:math>
 </para>
</solution>
</exercise><exercise id="fs-id1165134043550"><problem id="fs-id1165134043552">

