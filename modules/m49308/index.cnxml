<problem id="fs-id1165137647741">
<para id="fs-id1165137647743">Find <m:math>
<m:mrow><m:mo stretchy="false">(</m:mo><m:mi>f</m:mi><m:mo>∘</m:mo><m:mi>g</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo></m:mrow>
</m:math> and <m:math>
