<m:mrow>
<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn></m:mrow>
</m:mfrac>

</m:mtd>
</m:mtr>
</m:mtable>

</m:math>
</para>
</solution>
