<m:mi>x</m:mi>
<m:mn>2</m:mn>
</m:msup>
</m:mrow>
</m:math> and <m:math>
<m:mrow><m:mi>g</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msqrt>
<m:mrow>
<m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn></m:mrow>
</m:msqrt>
<m:mo>,</m:mo></m:mrow>
</m:math> find <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo>+</m:mo><m:mi>g</m:mi><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mi>f</m:mi><m:mo>−</m:mo><m:mi>g</m:mi><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mi>f</m:mi><m:mi>g</m:mi><m:mo>,</m:mo>
