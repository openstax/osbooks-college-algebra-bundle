<exercise id="fs-id1165137668139">
<problem id="fs-id1165135667825">
<para id="fs-id1165135667827"> <m:math>
 <m:mrow>
  <m:mo>{</m:mo><m:mtable columnalign="left">
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4</m:mn><m:mtext>cos</m:mtext><m:mspace width="0.3em"/><m:mi>t</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>5</m:mn><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mspace width="0.5em"/>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165137762341">
<para id="fs-id1165137762343"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mrow>
    <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mfrac>
       <m:mi>x</m:mi>
       <m:mn>4</m:mn>
      </m:mfrac>
      </m:mrow>
    <m:mo>)</m:mo></m:mrow></m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:msup>
   <m:mrow>
    <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mfrac>
       <m:mi>y</m:mi>
       <m:mn>5</m:mn>
      </m:mfrac>
      </m:mrow>
    <m:mo>)</m:mo></m:mrow></m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>=</m:mo><m:mn>1</m:mn></m:mrow>
</m:math>
</para>
</solution>
</exercise>
<exercise id="fs-id1165137585726">
<problem id="fs-id1165137585728">
<para id="fs-id1165131895934"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>3</m:mn><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi></m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>6</m:mn><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi></m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable></m:mrow> </m:mrow></m:mrow>
</m:math>
</para></problem>
</exercise>
<exercise id="fs-id1165135384981">
<problem id="fs-id1165135384983">
<para id="fs-id1165133210007"> <m:math>
 <m:mrow>
  <m:mo>{</m:mo><m:mtable columnalign="left">
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:msup>
       <m:mrow>
        <m:mtext>cos</m:mtext>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mi>t</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mspace width="0.5em"/>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165137602808">
<para id="fs-id1165137602810"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>y</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mfrac>
   <m:mn>1</m:mn>
   <m:mn>2</m:mn>
  </m:mfrac>
  <m:mi>x</m:mi></m:mrow>
</m:math>
</para>
</solution>
</exercise>
<exercise id="fs-id1165135189886">
<problem id="fs-id1165135189889">
<para id="fs-id1165135189891"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:msup>
      <m:mi>sin</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mi>t</m:mi>
    </m:mtd>
   </m:mtr>
  </m:mtable>
   </m:mrow></m:mrow>
</m:math>
</para></problem>
</exercise>
<exercise id="fs-id1165135193646">
<problem id="fs-id1165135193648">
<para id="fs-id1165135316171"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>t</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>
      <m:mi>t</m:mi>
      <m:mn>2</m:mn>
     </m:msup>

    </m:mtd>
   </m:mtr>
  </m:mtable>
   </m:mrow></m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165133213863">
<para id="fs-id1165133454382"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo>=</m:mo><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow>
</m:math>
</para>
</solution>
</exercise>
<exercise id="fs-id1165137411313">
<problem id="fs-id1165134037467">
<para id="fs-id1165134037469"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:mi>t</m:mi>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>
      <m:mi>t</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>1</m:mn>
    </m:mtd>
   </m:mtr>
  </m:mtable>
   </m:mrow></m:mrow>
</m:math>
</para>
</problem>
</exercise>
<exercise id="fs-id1165134319713">
<problem id="fs-id1165134319715">
<para id="fs-id1165135341173"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>t</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>
      <m:mi>t</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>2</m:mn>
    </m:mtd>
   </m:mtr>
  </m:mtable>
   </m:mrow></m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165137704906">
<para id="fs-id1165137704908"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo>=</m:mo><m:msup>
   <m:mrow>
    <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow>
       <m:mn>2</m:mn>
      </m:mfrac>
      </m:mrow>
    <m:mo>)</m:mo></m:mrow></m:mrow>
   <m:mn>3</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mn>2</m:mn></m:mrow>
</m:math>
</para>
</solution>
</exercise>

<para id="fs-id1165137548472">For the following exercises, rewrite the parametric equation as a Cartesian equation by building an <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mtext>-</m:mtext><m:mi>y</m:mi>
 </m:mrow>
</m:math>  table.</para><exercise id="fs-id1165137582353">
<problem id="fs-id1165137582355">
<para id="fs-id1165137582357"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>t</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>t</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>
    </m:mtd>
   </m:mtr>
  </m:mtable>
   </m:mrow></m:mrow>
</m:math>
</para>
</problem>
</exercise>
<exercise id="fs-id1165134239737">
<problem id="fs-id1165137735773">
<para id="fs-id1165137735775"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4</m:mn><m:mo>−</m:mo><m:mi>t</m:mi>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>3</m:mn><m:mi>t</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
    </m:mtd>
   </m:mtr>
  </m:mtable>
   </m:mrow></m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165134495094">
<para id="fs-id1165134495096"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>14</m:mn></m:mrow>
</m:math>
</para>
</solution>
</exercise>
<exercise id="fs-id1165132947372">
<problem id="fs-id1165132947374">
<para id="fs-id1165134109660"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>t</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>5</m:mn><m:mi>t</m:mi>
    </m:mtd>
   </m:mtr>
  </m:mtable>
   </m:mrow></m:mrow>
</m:math>
</para>
</problem>
</exercise>
<exercise id="fs-id1165134116716">
<problem id="fs-id1165134116719">
<para id="fs-id1165135173427"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mtable columnalign="left">
   <m:mtr>
    <m:mtd>
     <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4</m:mn><m:mi>t</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4</m:mn><m:mi>t</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
    </m:mtd>
   </m:mtr>
  </m:mtable>
   </m:mrow></m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165134187207">
<para id="fs-id1165135528264"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo>=</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn></m:mrow>
</m:math>
</para>
</solution>
</exercise>

<para id="fs-id1165135456667">For the following exercises, parameterize (write parametric equations for) each Cartesian equation by setting <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>t</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>t</m:mi></m:mrow>
</m:math>  or by setting <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>t</m:mi><m:mo>.</m:mo></m:mrow>
</m:math>
</para><exercise id="fs-id1165137595902">
<problem id="fs-id1165135329750">
<para id="fs-id1165135329752"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>3</m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>3</m:mn></m:mrow>
</m:math>
</para>
</problem>
</exercise>
<exercise id="fs-id1165134148489">
<problem id="fs-id1165134148491">
<para id="fs-id1165134148493"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165137925225">
<para id="fs-id1165137925228"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>t</m:mi></m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>sin</m:mi><m:mi>t</m:mi><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable></m:mrow> </m:mrow></m:mrow>
</m:math>
</para></solution>
</exercise>
<exercise id="fs-id1165134373445">
<problem id="fs-id1165134373447">
<para id="fs-id1165135641460"> <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>y</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>3</m:mn><m:mi>log</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>y</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mi>y</m:mi></m:mrow>
</m:math> </para></problem>
</exercise>
<exercise id="fs-id1165135582390">
<problem id="fs-id1165135582392">
<para id="fs-id1165135582394"> <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>y</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:msqrt>
   <m:mi>y</m:mi>
  </m:msqrt>
  <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>y</m:mi></m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165137575928">
<para id="fs-id1165137575930"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:msqrt>
        <m:mi>t</m:mi>
       </m:msqrt>
       <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>t</m:mi></m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>t</m:mi></m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable></m:mrow> </m:mrow></m:mrow>
</m:math>
</para>
</solution>
</exercise>
<para id="fs-id1165135188802">For the following exercises, parameterize (write parametric equations for) each Cartesian equation by using <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>t</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>a</m:mi><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi></m:mrow>
</m:math>  and <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>b</m:mi><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>.</m:mo></m:mrow>
</m:math> Identify the curve.</para><exercise id="fs-id1165137548806">
<problem id="fs-id1165137548808">
<para id="fs-id1165137548810"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    </m:mrow>
   <m:mn>4</m:mn>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>y</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    </m:mrow>
   <m:mn>9</m:mn>
  </m:mfrac>
  <m:mo>=</m:mo><m:mn>1</m:mn></m:mrow>
</m:math>
</para>
</problem>
</exercise>
