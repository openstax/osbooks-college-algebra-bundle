<exercise id="fs-id1165135581966">
<problem id="fs-id1165137401665">
<para id="fs-id1165137401667"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:msup>
        <m:mi>e</m:mi>
        <m:mi>t</m:mi>
       </m:msup>
       </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mn>5</m:mn><m:mi>t</m:mi></m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable></m:mrow> </m:mrow></m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165135496291">
<para id="fs-id1165135496294"> <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:msup>
   <m:mi>e</m:mi>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>1</m:mn><m:mo>−</m:mo><m:mi>y</m:mi></m:mrow>
     <m:mn>5</m:mn>
    </m:mfrac>
    </m:mrow>
  </m:msup>
  </m:mrow>
</m:math> or <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mn>5</m:mn><m:mi>l</m:mi><m:mi>n</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:mfrac>
    </m:mrow>
  <m:mo>)</m:mo></m:mrow></m:mrow>
</m:math>
</para>
</solution>
</exercise>
