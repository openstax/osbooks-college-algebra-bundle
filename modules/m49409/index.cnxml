 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:msup>
        <m:mi>t</m:mi>
        <m:mn>3</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:mi>t</m:mi></m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>t</m:mi>
       <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>t</m:mi></m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable></m:mrow> </m:mrow></m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165137803762">
<para id="fs-id1165137803764"> <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mo>=</m:mo><m:msup>
   <m:mrow>
    <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mfrac>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:mfrac>
      </m:mrow>
    <m:mo>)</m:mo></m:mrow></m:mrow>
   <m:mn>3</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mfrac>
   <m:mi>y</m:mi>
   <m:mn>2</m:mn>
  </m:mfrac>
  </m:mrow>
</m:math>
</para>
</solution>
