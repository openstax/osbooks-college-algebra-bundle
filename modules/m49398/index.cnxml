   <m:mrow>
    <m:mi>tan</m:mi>
   </m:mrow>
   <m:mrow>
<m:mn>−1</m:mn>
   </m:mrow>
  </m:msup>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
<m:mn>−2</m:mn>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow></m:mrow></m:math>, 
  
  <m:math><m:mi>π</m:mi><m:mo>+</m:mo><m:msup>
   <m:mrow>
    <m:mi>tan</m:mi>
   </m:mrow>
   <m:mrow>
<m:mn>−1</m:mn>
   </m:mrow>
  </m:msup>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mo>−</m:mo>
    <m:mfrac>
     <m:mn>3</m:mn>
     <m:mn>2</m:mn>
    </m:mfrac>

   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo></m:math>

  <m:math>
  <m:mn>2</m:mn><m:mi>π</m:mi><m:mo>+</m:mo><m:msup>
   <m:mrow>
    <m:mi>tan</m:mi>
   </m:mrow>
   <m:mrow>
<m:mn>−1</m:mn>
   </m:mrow>
  </m:msup>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
<m:mn>−2</m:mn>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo></m:math>
 <m:math>
 <m:mrow> 
  <m:mn>2</m:mn><m:mi>π</m:mi><m:mo>+</m:mo>
  <m:msup>
    <m:mi>tan</m:mi>
<m:mn>−1</m:mn>
  </m:msup>
  <m:mo>(</m:mo>
   <m:mrow>
