  <m:mn>10</m:mn><m:mspace width="0.5em"/><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mspace width="0.3em"/><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>6</m:mn><m:mspace width="0.5em"/><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id3275862"><para id="fs-id3275863"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mrow>
    <m:mi>sin</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>1</m:mn>
   </m:mrow>
  </m:msup>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mn>3</m:mn>
     <m:mn>5</m:mn>
    </m:mfrac>

   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mfrac>
   <m:mi>π</m:mi>
   <m:mn>2</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mi>π</m:mi><m:mo>−</m:mo><m:msup>
   <m:mrow>
    <m:mi>sin</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>1</m:mn>
   </m:mrow>
  </m:msup>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mn>3</m:mn>
     <m:mn>5</m:mn>
    </m:mfrac>

   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>3</m:mn><m:mi>π</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></solution></exercise>
