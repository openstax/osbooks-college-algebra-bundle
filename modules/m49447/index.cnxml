<m:mrow>
<m:msub>
<m:mi>S</m:mi>
<m:mn>5</m:mn>
</m:msub>
<m:mo>=</m:mo><m:mfrac>
<m:mrow>
<m:mn>5</m:mn><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mfrac>
<m:mn>3</m:mn>
<m:mn>2</m:mn>
</m:mfrac>
<m:mo>+</m:mo><m:mfrac>
<m:mn>7</m:mn>
<m:mn>2</m:mn>
</m:mfrac>

</m:mrow>
<m:mo>)</m:mo></m:mrow>
</m:mrow>
<m:mn>2</m:mn>
</m:mfrac>

</m:mrow>
