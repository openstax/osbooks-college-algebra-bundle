    <m:mn>8</m:mn><m:mo>⋅</m:mo><m:msup>
     <m:mrow>
      <m:mn>0.5</m:mn>
     </m:mrow>
     <m:mrow>
      <m:mi>k</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msup>

   </m:mrow>
  </m:mstyle>
 </m:mrow>
</m:math>
