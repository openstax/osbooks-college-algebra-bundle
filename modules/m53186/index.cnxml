</m:math> </equation>
<para id="fs-id1288087">Because <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>b</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:msup>
   <m:mi>c</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>=</m:mo><m:msup>
   <m:mi>a</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>,</m:mo>
 </m:mrow>
</m:math> then <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>b</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>=</m:mo><m:msup>
   <m:mi>a</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:msup>
   <m:mi>c</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>.</m:mo>
 </m:mrow>
</m:math> </para>
<equation id="fs-id1579078" class="unnumbered"><label/> <m:math>
 <m:mrow>
  <m:mtable>
   <m:mtr>
