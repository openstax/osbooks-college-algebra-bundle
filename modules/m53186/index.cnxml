   <m:mrow>
    <m:msup>
     <m:mi>a</m:mi>
     <m:mn>2</m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>
  <m:mo>.</m:mo>
 </m:mrow>
</m:math> </para>
<equation id="fs-id2515626" class="unnumbered"><label/>
 <m:math>
 <m:mrow>
  <m:mtable>
