        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mn>3</m:mn><m:mo>⋅</m:mo><m:mn>5</m:mn>
          </m:mrow>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mn>3</m:mn><m:mo>⋅</m:mo><m:mn>4</m:mn>
          </m:mrow>
