          </m:mrow>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mo>=</m:mo><m:mo> </m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
       <m:mtable columnalign="right">
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mn>3</m:mn><m:mo>⋅</m:mo><m:mn>8</m:mn>
