<example id="Example_09_05_05"><exercise id="fs-id1165135475905"><problem id="fs-id1165135475907">
    <title>Finding the Sum and Difference of Two 3 x 3 Matrices</title>
    <para id="fs-id1165137836555">Given <m:math>
      <m:mrow>
        <m:mi>A</m:mi>
      </m:mrow>
    </m:math> and <m:math>
      <m:mrow>
        <m:mi>B</m:mi><m:mo>:</m:mo>
      </m:mrow>
    </m:math>
    </para>
<list id="fs-id1165134389752" list-type="enumerated" number-style="arabic" class="circled">
<item><span class="token">ⓐ</span>Find the sum.</item>
<item><span class="token">ⓑ</span>Find the difference.</item>
</list>
    <equation id="fs-id1165137793728" class="unnumbered"><label/> <m:math display="block">
 <m:mrow>
  <m:mi>A</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable columnalign="right">
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>2</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−10</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−2</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>14</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>12</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>10</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>4</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−2</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>2</m:mn>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow><m:mspace width="0.8em"/><m:mtext>and </m:mtext><m:mi>B</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable columnalign="right">
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>6</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>10</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−2</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>0</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−12</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−4</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−5</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>2</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−2</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow>
 </m:mrow>
</m:math>
</equation></problem>
  <solution id="fs-id1165137468840">

