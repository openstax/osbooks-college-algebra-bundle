    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>0</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−3</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>2</m:mn>
