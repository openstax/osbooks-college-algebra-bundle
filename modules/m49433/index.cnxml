<example id="Example_09_05_04">
<exercise id="fs-id1165134216247">
  <problem id="fs-id1165134199499">
    <title>Finding the Difference of Two Matrices</title>
    <para id="fs-id1165135388477">Find the difference of <m:math>
      <m:mrow>
        <m:mi>A</m:mi>
      </m:mrow>
    </m:math> and <m:math>
      <m:mrow>
        <m:mi>B</m:mi><m:mo>.</m:mo>
      </m:mrow>
    </m:math>
    </para>
    <equation id="fs-id1165133212858" class="unnumbered"><label/> <m:math display="block">
 <m:mrow>
  <m:mi>A</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mn>−2</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mn>3</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mn>0</m:mn>
     </m:mtd>
     <m:mtd>
      <m:mn>1</m:mn>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow><m:mspace width="0.8em"/><m:mtext> and  </m:mtext><m:mi>B</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable>
    <m:mtr>
     <m:mtd>
