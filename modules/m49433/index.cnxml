        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>+</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
       <m:mtable>
        <m:mtr>
         <m:mtd>
          <m:mn>5</m:mn>
         </m:mtd>
         <m:mtd>
          <m:mn>9</m:mn>
         </m:mtd>
        </m:mtr>
        <m:mtr>
