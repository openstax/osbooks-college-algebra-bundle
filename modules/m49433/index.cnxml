    <example id="Example_09_05_06">
      <exercise id="fs-id1165135571889">
        <problem id="fs-id1165135571891">
          <title>Multiplying the Matrix by a Scalar</title>
          <para id="fs-id1165137531329">Multiply matrix <m:math>
            <m:mrow>
              <m:mi>A</m:mi>
            </m:mrow>
          </m:math> by the scalar 3.</para>
          <equation id="fs-id1165132032532" class="unnumbered"><label/> <m:math display="block">
            <m:mrow>
              <m:mi>A</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
                <m:mtable>
                  <m:mtr>
                    <m:mtd>
                      <m:mn>8</m:mn>
                    </m:mtd>
                    <m:mtd>
                      <m:mn>1</m:mn>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:mn>5</m:mn>
                    </m:mtd>
                    <m:mtd>
                      <m:mn>4</m:mn>
                    </m:mtd>
                  </m:mtr>

                </m:mtable>
              </m:mrow> <m:mo>]</m:mo></m:mrow>
            </m:mrow>
          </m:math>
          </equation>
        </problem>
        <solution id="fs-id1165133103962">


