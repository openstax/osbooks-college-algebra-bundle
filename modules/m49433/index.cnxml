      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mn>30</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mn>11</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>≠</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mn>−9</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mn>10</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mn>10</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mn>4</m:mn>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mn>−8</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mn>−12</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mrow>
