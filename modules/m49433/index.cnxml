            <m:mrow>
              <m:mn>2</m:mn><m:mi>B</m:mi><m:mo>.</m:mo>
            </m:mrow>
          </m:math>
          <newline count="1"/></para><equation id="fs-id1165135547024" class="unnumbered"><label/> <m:math display="block">
 <m:mrow>
