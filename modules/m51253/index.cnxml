<exercise id="fs-id2771146"><problem id="fs-id2771148"><para id="fs-id2771149"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>3</m:mn><m:mi>x</m:mi>
   </m:mrow>
