  </m:msub>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>3</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>1</m:mn>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
