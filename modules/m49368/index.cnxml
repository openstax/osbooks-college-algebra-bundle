  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mo>⋅</m:mo><m:msup>
   <m:mi>e</m:mi>
   <m:mrow>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
   </m:mrow>
  </m:msup>
