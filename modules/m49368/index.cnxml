  <m:mi>g</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>log</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mn>7</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>21</m:mn>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>−</m:mo><m:mn>4.</m:mn>
 </m:mrow>
</m:math> </para>
</problem>
