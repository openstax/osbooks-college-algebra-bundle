      <exercise id="fs-id1649477"><problem id="fs-id1649478"><para id="fs-id1649479"> <m:math>
 <m:mrow>
  <m:mi>r</m:mi><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>6</m:mn><m:mi>sec</m:mi><m:mspace width="0.8em"/><m:mtext/><m:mi>θ</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>+</m:mo><m:mn>3</m:mn><m:mspace width="0.8em"/><m:mtext/><m:mi>sec</m:mi><m:mspace width="0.8em"/><m:mtext/><m:mi>θ</m:mi>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></problem>
        <solution id="fs-id2439848">
          <para id="fs-id2439850"> <m:math>
            <m:mrow>
              <m:mn>5</m:mn><m:msup>
                <m:mi>x</m:mi>
                <m:mn>2</m:mn>
              </m:msup>
              <m:mo>+</m:mo><m:mn>9</m:mn><m:msup>
                <m:mi>y</m:mi>
                <m:mn>2</m:mn>
              </m:msup>
              <m:mo>−</m:mo><m:mn>24</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>36</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
            </m:mrow>
