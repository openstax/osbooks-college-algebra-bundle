                <m:mn>6</m:mn>
                <m:mn>2</m:mn>
              </m:msup>

            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>;</m:mo>
        </m:mrow>
      </m:math> center: <m:math>
        <m:mrow>
          <m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>4</m:mn><m:mo>,</m:mo><m:mn>−1</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo>
        </m:mrow>
      </m:math> vertices: <m:math>
        <m:mrow>
          <m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>4</m:mn><m:mo>,</m:mo><m:mn>3</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
              <m:mrow>
