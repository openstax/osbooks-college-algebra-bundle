     <exercise id="fs-id2146165"><problem id="fs-id2146166"><para id="fs-id2146167"> <m:math>
       <m:mrow>
         <m:mn>9</m:mn><m:msup>
           <m:mi>x</m:mi>
           <m:mn>2</m:mn>
         </m:msup>
         <m:mo>+</m:mo><m:msup>
           <m:mi>y</m:mi>
           <m:mn>2</m:mn>
         </m:msup>
         <m:mo>+</m:mo><m:mn>54</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>76</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
       </m:mrow>
     </m:math>
     </para></problem>
       <solution id="fs-id2146222">
         <para id="fs-id2146224"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mrow>
      <m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo>
     </m:mrow>
     <m:mn>2</m:mn>
    </m:msup>

   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mn>1</m:mn>
     <m:mn>2</m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mrow>
      <m:mo stretchy="false">(</m:mo><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo>
     </m:mrow>
     <m:mn>2</m:mn>
    </m:msup>

   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mn>3</m:mn>
     <m:mn>2</m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>
  <m:mo>=</m:mo><m:mn>1</m:mn><m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo><m:mo>;</m:mo><m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>5</m:mn><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>;</m:mo><m:mspace width="0.8em"/><m:mspace width="0.8em"/><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>2</m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:msqrt>
     <m:mn>2</m:mn>
    </m:msqrt>

   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>2</m:mn><m:mo>−</m:mo><m:mn>2</m:mn><m:msqrt>
     <m:mn>2</m:mn>
    </m:msqrt>

   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math>
</para></solution></exercise>
     <exercise id="fs-id2613644"><problem id="fs-id2613646"><para id="fs-id2613647"> <m:math>
