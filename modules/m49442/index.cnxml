        </m:mrow>
      </m:math> foci: <m:math>
        <m:mrow>
          <m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>3</m:mn><m:msqrt>
                <m:mn>3</m:mn>
              </m:msqrt>
              <m:mo>,</m:mo><m:mn>0</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mo>−</m:mo><m:mn>3</m:mn><m:msqrt>
                  <m:mn>3</m:mn>
                </m:msqrt>
                <m:mo>,</m:mo><m:mn>0</m:mn>
              </m:mrow>
              <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math>
