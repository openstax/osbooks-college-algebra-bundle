            <m:mn>2</m:mn>
          </m:msup>
          <m:mo>+</m:mo><m:mn>24</m:mn><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>9</m:mn><m:msup>
            <m:mi>y</m:mi>
            <m:mn>2</m:mn>
          </m:msup>
          <m:mo>=</m:mo><m:mn>4</m:mn>
        </m:mrow>
      </m:math>
      </para></problem>
        <solution id="fs-id1237496">
          <para id="fs-id1237498"> <m:math>
            <m:mrow>
              <m:mi>r</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mfrac>
                <m:mn>2</m:mn>
                <m:mrow>
                  <m:mn>4</m:mn><m:mi>cos</m:mi><m:mi>θ</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:mi>sin</m:mi><m:mi>θ</m:mi>
                </m:mrow>
              </m:mfrac>

            </m:mrow>
          </m:math>
          </para>
        </solution></exercise>
      <exercise id="fs-id1237539"><problem id="fs-id1237540"><para id="fs-id1237541"> <m:math>
        <m:mrow>
          <m:mn>2</m:mn><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
        </m:mrow>
