     </m:math>
     </para></problem>
    <solution id="fs-id2888442">
      <para id="fs-id2888444">center: <m:math>
        <m:mrow>
          <m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>0</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo>
        </m:mrow>
      </m:math> vertices: <m:math>
        <m:mrow>
          <m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>6</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
              <m:mrow>
<m:mn>−6</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
              </m:mrow>
              <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mn>0</m:mn><m:mo>,</m:mo><m:mn>3</m:mn>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
