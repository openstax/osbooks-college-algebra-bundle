              </m:msqrt>

            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mn>4</m:mn><m:mo>,</m:mo><m:mn>−1</m:mn><m:mo>−</m:mo><m:mn>2</m:mn><m:msqrt>
                  <m:mrow>
