                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
                <m:mn>2</m:mn>
              </m:msup>

            </m:mrow>
            <m:mrow>
              <m:mn>16</m:mn>
            </m:mrow>
          </m:mfrac>
          <m:mo>−</m:mo><m:mfrac>
            <m:mrow>
              <m:msup>
                <m:mrow>
