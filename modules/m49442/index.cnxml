    <solution id="fs-id1408196">
      <para id="fs-id1408198"> <m:math>
        <m:mrow>
          <m:mfrac>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
                <m:mn>2</m:mn>
              </m:msup>

            </m:mrow>
            <m:mrow>
              <m:msup>
                <m:mn>4</m:mn>
                <m:mn>2</m:mn>
              </m:msup>

            </m:mrow>
          </m:mfrac>
          <m:mo>−</m:mo><m:mfrac>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
                <m:mn>2</m:mn>
              </m:msup>

            </m:mrow>
            <m:mrow>
