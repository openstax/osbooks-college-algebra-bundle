<exercise id="fs-id1487491"><problem id="fs-id1487492"><para id="fs-id1487493"> <m:math>
 <m:mrow>
  <m:msqrt>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>27</m:mn>
     </m:mrow>
     <m:mrow>
