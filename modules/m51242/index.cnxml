<exercise id="fs-id1167339438550"><problem id="fs-id1167339438552"><para id="fs-id1167339438553"> <m:math>
 <m:mrow>
  <m:msqrt>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mroot>
       <m:mrow>
        <m:mn>64</m:mn>
       </m:mrow>
       <m:mn>3</m:mn>
      </m:mroot>
      <m:mo>+</m:mo><m:mroot>
       <m:mrow>
        <m:mn>256</m:mn>
       </m:mrow>
       <m:mn>4</m:mn>
      </m:mroot>

     </m:mrow>
     <m:mrow>
      <m:msqrt>
       <m:mrow>
        <m:mn>64</m:mn>
       </m:mrow>
      </m:msqrt>
      <m:mo>+</m:mo><m:msqrt>
       <m:mrow>
        <m:mn>256</m:mn>
       </m:mrow>
      </m:msqrt>

     </m:mrow>
    </m:mfrac>

   </m:mrow>
  </m:msqrt>

 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1167339438628"><para id="fs-id1167339438629"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msqrt>
     <m:mn>3</m:mn>
    </m:msqrt>

   </m:mrow>
   <m:mn>3</m:mn>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></solution></exercise>

