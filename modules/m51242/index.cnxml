  </m:mfrac>

 </m:mrow>
</m:math>
</para></problem></exercise>
<exercise id="fs-id1167339438131"><problem id="fs-id1167339438132"><para id="fs-id1167339438133"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msqrt>
     <m:mrow>
      <m:mi>m</m:mi><m:msup>
       <m:mi>n</m:mi>
       <m:mn>3</m:mn>
      </m:msup>

     </m:mrow>
    </m:msqrt>

   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>a</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:msqrt>
     <m:mrow>
      <m:msup>
       <m:mi>c</m:mi>
       <m:mrow>
        <m:mn>−3</m:mn>
       </m:mrow>
      </m:msup>

     </m:mrow>
    </m:msqrt>

   </m:mrow>
  </m:mfrac>
  <m:mo>⋅</m:mo><m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>a</m:mi>
     <m:mrow>
      <m:mn>−7</m:mn>
     </m:mrow>
    </m:msup>
    <m:msup>
     <m:mi>n</m:mi>
     <m:mrow>
      <m:mn>−2</m:mn>
     </m:mrow>
    </m:msup>

   </m:mrow>
   <m:mrow>
    <m:msqrt>
     <m:mrow>
      <m:msup>
       <m:mi>m</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:msup>
       <m:mi>c</m:mi>
       <m:mn>4</m:mn>
      </m:msup>

     </m:mrow>
    </m:msqrt>

   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1167339438268"><para id="fs-id1167339438269"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msqrt>
     <m:mrow>
      <m:mi>m</m:mi><m:mi>n</m:mi><m:mi>c</m:mi>
     </m:mrow>
    </m:msqrt>

   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>a</m:mi>
     <m:mn>9</m:mn>
    </m:msup>
    <m:mi>c</m:mi><m:mi>m</m:mi><m:mi>n</m:mi>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339438318"><problem id="fs-id1167339438319"><para id="fs-id1167339438320"> <m:math>
 <m:mrow>
