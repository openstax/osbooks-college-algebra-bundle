    </m:msup>

   </m:mrow>
  </m:msqrt>

 </m:mrow>
</m:math>
</item>
</list>

</problem>
<solution id="fs-id1323031">
<list id="fs-id1372340" list-type="enumerated" number-style="arabic" class="circled">
  <item><span class="token">ⓐ</span><newline/> <m:math>
 <m:mrow>
  <m:mtable>
