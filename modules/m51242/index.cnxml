      </m:msqrt>

     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow><m:mspace width="4.5em"/>
      <m:mtext>Simplify</m:mtext><m:mo>.</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>

</item>
<item><span class="token">ⓑ</span><newline/> <m:math>
 <m:mrow>
  <m:mtable>
   <m:mtr>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:msqrt>
       <m:mrow>
        <m:mn>81</m:mn><m:msup>
         <m:mi>a</m:mi>
         <m:mn>4</m:mn>
        </m:msup>
        <m:msup>
         <m:mi>b</m:mi>
         <m:mn>4</m:mn>
        </m:msup>
        <m:mo>⋅</m:mo><m:mn>2</m:mn><m:mi>a</m:mi>
       </m:mrow>
      </m:msqrt>

     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow><m:mspace width="2em"/>
      <m:mtext>Factor perfect square from radicand</m:mtext><m:mo>.</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:msqrt>
       <m:mrow>
        <m:mn>81</m:mn><m:msup>
         <m:mi>a</m:mi>
         <m:mn>4</m:mn>
        </m:msup>
        <m:msup>
         <m:mi>b</m:mi>
         <m:mn>4</m:mn>
        </m:msup>

       </m:mrow>
      </m:msqrt>
      <m:mo>⋅</m:mo><m:msqrt>
       <m:mrow>
        <m:mn>2</m:mn><m:mi>a</m:mi>
