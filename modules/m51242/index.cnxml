       </m:mrow>
      </m:msqrt>
      <m:mo>⋅</m:mo><m:msqrt>
       <m:mn>3</m:mn>
      </m:msqrt>

     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow><m:mspace width="4.5em"/>
      <m:mtext>Write radical expression as product of radical expressions</m:mtext><m:mo>.</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd columnalign="left">
     <m:mrow>
