<exercise id="fs-id1167339438349"><problem id="fs-id1167339438350"><para id="fs-id1167339438351"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mi>x</m:mi><m:msqrt>
     <m:mrow>
      <m:mn>64</m:mn><m:mi>y</m:mi>
     </m:mrow>
    </m:msqrt>
    <m:mo>+</m:mo><m:mn>4</m:mn><m:msqrt>
     <m:mi>y</m:mi>
    </m:msqrt>

   </m:mrow>
   <m:mrow>
    <m:msqrt>
     <m:mrow>
      <m:mn>128</m:mn><m:mi>y</m:mi>
     </m:mrow>
    </m:msqrt>

   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1167339438405"><para id="fs-id1167339438406"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>2</m:mn><m:msqrt>
     <m:mn>2</m:mn>
    </m:msqrt>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:msqrt>
     <m:mn>2</m:mn>
    </m:msqrt>

   </m:mrow>
   <m:mn>4</m:mn>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339438444"><problem id="fs-id1167339438445"><para id="fs-id1167339438446"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
