   </exercise>

   <exercise id="fs-id1165137793832">
    <problem id="fs-id1165134393100">
     <para id="fs-id1165134393102"> <m:math>
      <m:mrow>
       <m:mrow><m:mo>|</m:mo> <m:mrow>
        <m:mfrac>
         <m:mn>1</m:mn>
         <m:mn>2</m:mn>
        </m:mfrac>
        <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn></m:mrow> <m:mo>|</m:mo></m:mrow><m:mo>=</m:mo><m:mn>11</m:mn></m:mrow>
     </m:math>
     </para>
    </problem>
   </exercise>
   <exercise id="fs-id1165137401505">
    <problem id="fs-id1165137401507">
     <para id="fs-id1165137852038"> <m:math>
      <m:mrow>
       <m:mrow><m:mo>|</m:mo> <m:mrow>
        <m:mfrac>
         <m:mn>1</m:mn>
         <m:mn>3</m:mn>
        </m:mfrac>
        <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn></m:mrow> <m:mo>|</m:mo></m:mrow><m:mo>=</m:mo><m:mn>14</m:mn></m:mrow>
     </m:math>
     </para>
    </problem>
    <solution id="fs-id1165137854997">
     <para id="fs-id1165137564109"> <m:math>
      <m:mrow>
       <m:mrow><m:mo>{</m:mo> <m:mrow>
        <m:mo>−</m:mo><m:mn>57</m:mn><m:mo>,</m:mo><m:mn>27</m:mn></m:mrow> <m:mo>}</m:mo></m:mrow></m:mrow>
     </m:math>
     </para>
    </solution>
   </exercise>

   <exercise id="fs-id1165135160638">
    <problem id="fs-id1165137769919">
     <para id="fs-id1165137769921"> <m:math>
      <m:mrow>
       <m:mo>−</m:mo><m:mrow><m:mo>|</m:mo> <m:mrow>
        <m:mfrac>
         <m:mn>1</m:mn>
         <m:mn>3</m:mn>
        </m:mfrac>
        <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn></m:mrow> <m:mo>|</m:mo></m:mrow><m:mo>+</m:mo><m:mn>14</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow>
     </m:math>
     </para>
    </problem>
   </exercise>

   <para id="fs-id1165137589731">For the following exercises, find the <emphasis effect="italics">x</emphasis>- and <emphasis effect="italics">y</emphasis>-intercepts of the graphs of each function.</para><exercise id="fs-id1165135190614">
    <problem id="fs-id1165134117307">
     <para id="fs-id1165134117309"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:mrow><m:mo>|</m:mo> <m:mrow>
   <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
  </m:mrow> <m:mo>|</m:mo></m:mrow><m:mo>−</m:mo><m:mn>10</m:mn>
 </m:mrow>
</m:math>
</para></problem>
   <solution id="fs-id1165134557389">
    <para id="fs-id1165134557391"> <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>0</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>8</m:mn></m:mrow>
       <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo><m:mspace width="0.5em"/><m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>6</m:mn><m:mo>,</m:mo><m:mn>0</m:mn></m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mspace width="0.5em"/><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>4</m:mn><m:mo>,</m:mo><m:mn>0</m:mn></m:mrow>
         <m:mo>)</m:mo></m:mrow></m:mrow>
    </m:math>
    </para>
   </solution>
   </exercise>

  <exercise id="fs-id1165134401702">
   <problem id="fs-id1165135362510">
    <para id="fs-id1165135362512"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4</m:mn><m:mrow><m:mo>|</m:mo> <m:mrow>
   <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
