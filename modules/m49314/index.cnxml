 <m:mrow>
  <m:mrow><m:mo>|</m:mo> <m:mrow>
   <m:mfrac>
    <m:mn>3</m:mn>
    <m:mn>4</m:mn>
   </m:mfrac>
   <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
  </m:mrow> <m:mo>|</m:mo></m:mrow><m:mo>≥</m:mo><m:mn>7</m:mn>
 </m:mrow>
</m:math>
</para></problem>
    <solution id="fs-id1165135377121">
     <para id="fs-id1165135377124"> <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo> <m:mrow>
        <m:mo>−</m:mo><m:mi>∞</m:mi><m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
         <m:mn>8</m:mn>
         <m:mn>3</m:mn>
        </m:mfrac>
       </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>∪</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
        <m:mn>16</m:mn><m:mo>,</m:mo><m:mi>∞</m:mi></m:mrow> <m:mo>)</m:mo></m:mrow></m:mrow>
     </m:math>
     </para>
    </solution>
   </exercise>

