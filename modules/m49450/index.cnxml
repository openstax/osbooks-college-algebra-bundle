<m:mn>2</m:mn>
<m:mrow>
<m:mn>50</m:mn>
</m:mrow>
</m:msup>
<m:mo>=</m:mo><m:mn>1.13</m:mn><m:mo>×</m:mo><m:msup>
<m:mrow>
<m:mn>10</m:mn>
