     <m:mi>x</m:mi>
     <m:mrow>
      <m:mn>14</m:mn>
     </m:mrow>
    </m:msup>

   </m:mrow>
   <m:mrow>
    <m:mn>16</m:mn>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para>
</solution>
</exercise>

