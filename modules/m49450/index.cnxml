<m:mrow><m:mo>{</m:mo> <m:mrow>
<m:mn>1</m:mn><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mn>3</m:mn><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mn>5</m:mn><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mo>…</m:mo><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mn>99</m:mn>
