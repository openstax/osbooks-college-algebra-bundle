            <m:mtr rowalign="center">
              <m:mtd rowalign="center"/>
              <m:mtd rowalign="center">
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left" rowalign="center">
                <m:mrow>
                  <m:mfrac>
                    <m:mrow>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mrow>
                          <m:mn>2</m:mn><m:mo>⋅</m:mo><m:mn>7</m:mn>
                        </m:mrow>
                      </m:msup>

                    </m:mrow>
                    <m:mrow>
                      <m:msup>
                        <m:mi>e</m:mi>
                        <m:mrow>
                          <m:mn>2</m:mn><m:mo>⋅</m:mo><m:mn>7</m:mn>
                        </m:mrow>
                      </m:msup>

                    </m:mrow>
                  </m:mfrac>

                </m:mrow>
              </m:mtd>
            </m:mtr>
            <m:mtr rowalign="center">
              <m:mtd rowalign="center"/>
              <m:mtd rowalign="center">
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left" rowalign="center">
                <m:mrow>
                  <m:mfrac>
                    <m:mrow>
                      <m:msup>
                        <m:mi>f</m:mi>
