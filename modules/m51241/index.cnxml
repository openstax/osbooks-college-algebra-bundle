         </m:math>
         </item>
         <item><span class="token">ⓒ</span> <m:math>
           <m:mrow>
             <m:mfrac>
               <m:mrow>
                 <m:msup>
                   <m:mrow>
                     <m:mrow><m:mo>(</m:mo>
                       <m:mrow>
                         <m:mn>−5</m:mn><m:msup>
                           <m:mi>t</m:mi>
