                       </m:mrow>
                       <m:mo>)</m:mo></m:mrow>
                   </m:mrow>
                   <m:mn>4</m:mn>
                 </m:msup>

               </m:mrow>
               <m:mrow>
                 <m:msup>
                   <m:mrow>
                     <m:mrow><m:mo>(</m:mo>
                       <m:mrow>
                         <m:mn>−5</m:mn><m:msup>
                           <m:mi>t</m:mi>
                           <m:mn>3</m:mn>
                         </m:msup>

                       </m:mrow>
                       <m:mo>)</m:mo></m:mrow>
                   </m:mrow>
                   <m:mn>8</m:mn>
                 </m:msup>

               </m:mrow>
             </m:mfrac>
             <m:mo>=</m:mo><m:msup>
               <m:mrow>
                 <m:mrow><m:mo>(</m:mo>
                   <m:mrow>
                     <m:mn>−5</m:mn><m:msup>
                       <m:mi>t</m:mi>
                       <m:mn>3</m:mn>
                     </m:msup>

                   </m:mrow>
                   <m:mo>)</m:mo></m:mrow>
               </m:mrow>
               <m:mrow>
                 <m:mn>4</m:mn><m:mo>−</m:mo><m:mn>8</m:mn>
               </m:mrow>
             </m:msup>
             <m:mo>=</m:mo><m:msup>
               <m:mrow>
                 <m:mrow><m:mo>(</m:mo>
                   <m:mrow>
                     <m:mn>−5</m:mn><m:msup>
                       <m:mi>t</m:mi>
                       <m:mn>3</m:mn>
                     </m:msup>

                   </m:mrow>
                   <m:mo>)</m:mo></m:mrow>
               </m:mrow>
               <m:mrow>
                 <m:mn>−4</m:mn>
               </m:mrow>
             </m:msup>
             <m:mo>=</m:mo><m:mfrac>
               <m:mn>1</m:mn>
               <m:mrow>
                 <m:msup>
                   <m:mrow>
                     <m:mrow><m:mo>(</m:mo>
                       <m:mrow>
                         <m:mn>−5</m:mn><m:msup>
                           <m:mi>t</m:mi>
                           <m:mn>3</m:mn>
                         </m:msup>

                       </m:mrow>
                       <m:mo>)</m:mo></m:mrow>
