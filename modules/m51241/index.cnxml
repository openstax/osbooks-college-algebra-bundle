                  </m:msup>

                </m:mrow>
                <m:mrow>
                  <m:msup>
                    <m:mi>w</m:mi>
                    <m:mn>6</m:mn>
                  </m:msup>

                </m:mrow>
              </m:mfrac>

            </m:mrow>
          </m:math>
          </item>
          <item><span class="token">ⓓ</span>
 <m:math>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
                  <m:msup>
                    <m:mi>t</m:mi>
                    <m:mn>3</m:mn>
                  </m:msup>
                  <m:mo>⋅</m:mo><m:msup>
