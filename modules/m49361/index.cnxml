 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>a</m:mi><m:msup>
   <m:mi>b</m:mi>
   <m:mi>x</m:mi>
  </m:msup>
  <m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext>where </m:mtext><m:mi>a</m:mi><m:mo>></m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mi>b</m:mi><m:mo>></m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mi>b</m:mi><m:mo>≠</m:mo><m:mn>1</m:mn>
 </m:mrow>
</m:math> </entry>
