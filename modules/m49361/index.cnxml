<m:mi>P</m:mi><m:mo>=</m:mo><m:mi>A</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>⋅</m:mo><m:msup>
<m:mrow>
<m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mn>1</m:mn><m:mo>+</m:mo><m:mfrac>
<m:mi>r</m:mi>
<m:mi>n</m:mi>
</m:mfrac>

</m:mrow>
<m:mo>)</m:mo></m:mrow>
