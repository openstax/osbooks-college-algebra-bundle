        <m:mi>n</m:mi><m:mi>t</m:mi>
       </m:mrow>
      </m:msup>
      <m:mo> </m:mo><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext>where</m:mtext>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>A</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mspace width="0.8em"/><m:mtext>is the account value at time </m:mtext><m:mi>t</m:mi>
