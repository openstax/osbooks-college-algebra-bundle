<m:mrow><m:mo>(</m:mo>
<m:mi>e</m:mi>
<m:mo>)</m:mo></m:mrow>
</m:mrow>
<m:mrow>
<m:mn>0.75</m:mn><m:mi>t</m:mi>
</m:mrow>
</m:msup>

</m:mrow>
</m:math> </para>
</problem>
