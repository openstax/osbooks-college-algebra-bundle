      <exercise id="fs-id1165131940267">
        <problem id="fs-id1165131940269">
          <para id="fs-id1165131940271"> <m:math>
            <m:mrow>
              <m:mi>α</m:mi><m:mo>=</m:mo><m:mn>43.1°</m:mn><m:mo>,</m:mo><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>184.2</m:mn><m:mo>,</m:mo><m:mi>b</m:mi><m:mo>=</m:mo><m:mn>242.8</m:mn></m:mrow>
          </m:math>
