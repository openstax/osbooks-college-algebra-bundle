      <exercise id="fs-id1165135538836">
        <problem id="fs-id1165135538838">
          <para id="fs-id1165135538840"> <m:math>
            <m:mrow>
              <m:mi>α</m:mi><m:mo>=</m:mo><m:mn>36.6°</m:mn><m:mo>,</m:mo><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>186.2</m:mn><m:mo>,</m:mo><m:mi>b</m:mi><m:mo>=</m:mo><m:mn>242.2</m:mn></m:mrow>
          </m:math>
          </para></problem>
      </exercise>
      <exercise id="fs-id1165132945436">
        <problem id="fs-id1165132945439">
          <para id="fs-id1165132945441"> <m:math>
            <m:mrow>
              <m:mi>β</m:mi><m:mo>=</m:mo>
                <m:mrow>
                  <m:mn>50°</m:mn></m:mrow>
              <m:mo>,</m:mo><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>105</m:mn><m:mo>,</m:mo><m:mi>b</m:mi><m:mo>=</m:mo><m:mn>45</m:mn><m:mmultiscripts>
