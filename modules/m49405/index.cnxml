   <m:mn>1</m:mn>
   <m:mn>3</m:mn>
  </m:mfrac>
  <m:mspace width="0.3em"/><m:mtext>m</m:mtext><m:mo>,</m:mo><m:mi>c</m:mi><m:mo>=</m:mo><m:mfrac>
   <m:mn>1</m:mn>
   <m:mn>4</m:mn>
  </m:mfrac>
  <m:mspace width="0.3em"/><m:mtext>m</m:mtext>
 </m:mrow>
</m:math>
</para></problem>
