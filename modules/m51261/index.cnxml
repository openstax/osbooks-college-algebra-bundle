<solution id="fs-id1165135357153">
 <para id="fs-id1165135357155"><m:math>
 <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:mo>;</m:mo></m:math>


    <m:math><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:mo>;</m:mo>
   </m:math>

<newline/>
   <m:math>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>a</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>|</m:mo>
        <m:mo>−</m:mo><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>1</m:mn> <m:mo>|</m:mo><m:mo>−</m:mo><m:mo>|</m:mo>
         <m:mo>−</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>1</m:mn> <m:mo>|</m:mo><m:mo>;</m:mo>
</m:math><newline/>
  <m:math>
       <m:mo>−</m:mo><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:mo>|</m:mo>
        <m:mi>a</m:mi><m:mo>−</m:mo><m:mn>1</m:mn> <m:mo>|</m:mo><m:mo>+</m:mo><m:mo>|</m:mo>
         <m:mi>a</m:mi><m:mo>+</m:mo><m:mn>1</m:mn> <m:mo>|</m:mo><m:mo>;</m:mo></m:math><newline/>
<m:math>
<m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>|</m:mo>
          <m:mi>a</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo>−</m:mo><m:mn>1</m:mn> <m:mo>|</m:mo><m:mo>−</m:mo><m:mo>|</m:mo>
           <m:mi>a</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo>+</m:mo><m:mn>1</m:mn> <m:mo>|</m:mo>
