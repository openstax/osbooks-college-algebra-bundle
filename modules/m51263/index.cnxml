     <m:mrow>
  <m:mo stretchy="false">[</m:mo><m:mn>−3</m:mn><m:mo>,</m:mo><m:mtext>1</m:mtext><m:mo stretchy="false">]</m:mo></m:mrow>
    </m:math>
    </para></problem>
   <solution id="fs-id1165135208852">
