    <m:mtd><m:mo>=</m:mo></m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mfrac>
       <m:mi>h</m:mi>
       <m:mrow>
        <m:mn>30</m:mn></m:mrow>
      </m:mfrac>
      </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="1em"/><m:mtext>Solve for </m:mtext><m:mi>h</m:mi><m:mo>.</m:mo></m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd columnalign="right">
     <m:mi>h</m:mi>
    </m:mtd>
    <m:mtd>
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>30tan</m:mtext><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mtext>57°</m:mtext></m:mrow>
      <m:mo>)</m:mo></m:mrow></m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="1em"/><m:mtext>Multiply</m:mtext><m:mtext>.</m:mtext></m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd columnalign="right">
     <m:mi>h</m:mi>
    </m:mtd>
    <m:mtd>
     <m:mo>≈</m:mo>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>46.2</m:mn></m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="1em"/><m:mtext>Use a calculator</m:mtext><m:mtext>.</m:mtext></m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable></m:mrow>
</m:math>
</equation><para id="fs-id1629300">The tree is approximately 46 feet tall.</para>
</solution>
</exercise>
