<problem id="fs-id1165137643357">
<para id="fs-id1165137643360"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mo>−</m:mo><m:mi>π</m:mi></m:mrow>
   <m:mn>6</m:mn>
  </m:mfrac>
  <m:mo>&lt;</m:mo><m:mi>θ</m:mi><m:mo>≤</m:mo><m:mfrac>
