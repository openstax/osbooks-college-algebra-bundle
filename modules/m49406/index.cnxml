  </m:mfrac>
  <m:mo>,</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>&lt;</m:mo><m:mi>r</m:mi><m:mspace width="0.3em"/><m:mo>&lt;</m:mo><m:mspace width="0.3em"/><m:mn>2</m:mn></m:mrow>
</m:math>
