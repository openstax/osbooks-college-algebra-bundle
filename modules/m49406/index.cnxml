<exercise id="fs-id1165131880477"><problem id="fs-id1165131880479">
<para id="fs-id1165131880481"> <m:math>
 <m:mrow>
  <m:mn>0</m:mn><m:mo>≤</m:mo><m:mi>θ</m:mi><m:mo>≤</m:mo><m:mfrac>
   <m:mi>π</m:mi>
   <m:mn>3</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mspace width="0.5em"/><m:mi>r</m:mi><m:mspace width="0.3em"/><m:mo>&lt;</m:mo><m:mspace width="0.3em"/><m:mn>2</m:mn></m:mrow>
