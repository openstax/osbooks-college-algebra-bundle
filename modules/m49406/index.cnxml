 <m:mrow>
  <m:mi>θ</m:mi><m:mo>=</m:mo><m:mfrac>
   <m:mi>π</m:mi>
   <m:mn>4</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mspace width="0.5em"/><m:mi>r</m:mi><m:mspace width="0.3em"/><m:mo>≥</m:mo><m:mspace width="0.3em"/><m:mn>2</m:mn></m:mrow>
</m:math>
