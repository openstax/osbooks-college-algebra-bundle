        </m:mrow>
      </m:math> and <m:math>
        <m:mrow>
          <m:mi>b</m:mi>
        </m:mrow>
      </m:math> both in the interval <m:math>
        <m:mrow>
          <m:mrow><m:mo>[</m:mo> <m:mrow>
            <m:mn>0</m:mn><m:mo>,</m:mo><m:mfrac>
              <m:mi>π</m:mi>
              <m:mn>2</m:mn>
            </m:mfrac>

          </m:mrow> <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
        </m:mrow>
      </m:math> find <m:math>
        <m:mrow>
          <m:mi>sin</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo>
        </m:mrow>
      </m:math> and <m:math>
        <m:mrow>
          <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo>
        </m:mrow>
      </m:math>
      </para></problem>
