    </m:mtd>
    <m:mtd columnalign="left" rowalign="center">
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mfrac>
         <m:mn>3</m:mn>
         <m:mn>5</m:mn>
        </m:mfrac>

       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mo>−</m:mo><m:mfrac>
         <m:mn>5</m:mn>
         <m:mrow>
          <m:mn>13</m:mn>
         </m:mrow>
        </m:mfrac>

       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mfrac>
         <m:mn>4</m:mn>
         <m:mn>5</m:mn>
        </m:mfrac>

       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mo>−</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>12</m:mn>
         </m:mrow>
         <m:mrow>
          <m:mn>13</m:mn>
         </m:mrow>
        </m:mfrac>

       </m:mrow>
      <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr rowalign="center">
    <m:mtd rowalign="center"/>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left" rowalign="center">
     <m:mrow>
      <m:mo>−</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>15</m:mn>
       </m:mrow>
       <m:mrow>
        <m:mn>65</m:mn>
       </m:mrow>
      </m:mfrac>
      <m:mo>−</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>48</m:mn>
       </m:mrow>
       <m:mrow>
        <m:mn>65</m:mn>
       </m:mrow>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr rowalign="center">
    <m:mtd rowalign="center"/>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left" rowalign="center">
     <m:mrow>
      <m:mo>−</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>63</m:mn>
       </m:mrow>
       <m:mrow>
        <m:mn>65</m:mn>
       </m:mrow>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>

</equation></item>
          <item><span class="token">ⓑ</span> We can find <m:math>
            <m:mrow>
              <m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>α</m:mi><m:mo>+</m:mo><m:mi>β</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow>
            </m:mrow>
          </m:math> in a similar manner. We substitute the values according to the formula.
            <equation id="fs-id1487615" class="unnumbered"><label/> <m:math display="block">
 <m:mrow>
  <m:mtable>
   <m:mtr rowalign="center">
    <m:mtd columnalign="right" rowalign="center">
     <m:mrow>
      <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>α</m:mi><m:mo>+</m:mo><m:mi>β</m:mi><m:mo stretchy="false">)</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left" rowalign="center">
     <m:mrow>
      <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>α</m:mi><m:mspace width="0.3em"/><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>β</m:mi><m:mo>−</m:mo><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>α</m:mi><m:mspace width="0.3em"/><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>β</m:mi>
