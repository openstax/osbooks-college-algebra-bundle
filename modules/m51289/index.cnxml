 </m:mrow>
</m:math>
</para></problem>
      <solution id="fs-id2884046">
        <para id="fs-id2884049"> <m:math>
 <m:mrow>
  <m:mtable>
   <m:mtr rowalign="center">
    <m:mtd columnalign="right" rowalign="center">
     <m:mrow>
      <m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mi>x</m:mi><m:mo>+</m:mo><m:mfrac>
         <m:mi>π</m:mi>
         <m:mn>4</m:mn>
        </m:mfrac>

       </m:mrow>
      <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd rowalign="center"/>
   </m:mtr>
   <m:mtr rowalign="center">
    <m:mtd columnalign="right" rowalign="center">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mi>tan</m:mi><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mfrac>
           <m:mi>π</m:mi>
           <m:mn>4</m:mn>
          </m:mfrac>

         </m:mrow>
        <m:mo>)</m:mo></m:mrow>
       </m:mrow>
       <m:mrow>
        <m:mn>1</m:mn><m:mo>−</m:mo><m:mi>tan</m:mi><m:mi>x</m:mi><m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mfrac>
           <m:mi>π</m:mi>
           <m:mn>4</m:mn>
          </m:mfrac>

         </m:mrow>
        <m:mo>)</m:mo></m:mrow>
       </m:mrow>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd rowalign="center"/>
   </m:mtr>
   <m:mtr rowalign="center">
    <m:mtd columnalign="right" rowalign="center">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mi>tan</m:mi><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
       </m:mrow>
       <m:mrow>
        <m:mn>1</m:mn><m:mo>−</m:mo><m:mi>tan</m:mi><m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
       </m:mrow>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left" rowalign="center">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mi>tan</m:mi><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
       </m:mrow>
       <m:mrow>
        <m:mn>1</m:mn><m:mo>−</m:mo><m:mi>tan</m:mi><m:mi>x</m:mi>
       </m:mrow>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></solution></exercise>
      <exercise id="fs-id1743113"><problem id="fs-id1743114"><para id="fs-id1743115"> <m:math>
