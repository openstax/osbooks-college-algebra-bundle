            <m:mn>5</m:mn>
          </m:mfrac>
          <m:mo>,</m:mo>
        </m:mrow>
      </m:math> and <m:math>
        <m:mrow>
          <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>b</m:mi><m:mo>=</m:mo><m:mfrac>
            <m:mn>1</m:mn>
            <m:mn>3</m:mn>
          </m:mfrac>
          <m:mo>,</m:mo>
        </m:mrow>
      </m:math> with <m:math>
        <m:mrow>
