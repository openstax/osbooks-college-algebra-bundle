       </m:mrow>
       <m:mrow>
        <m:mn>15</m:mn>
       </m:mrow>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>

        </para></solution></exercise>

