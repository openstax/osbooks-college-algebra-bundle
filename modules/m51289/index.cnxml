      <exercise id="fs-id1831266" class="material-set-2"><problem id="fs-id1831267"><para id="fs-id1831268"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo stretchy="false">)</m:mo><m:mo>−</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
   </m:mrow>
   <m:mi>h</m:mi>
  </m:mfrac>
  <m:mo>=</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mfrac>
   <m:mrow>
    <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>h</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
   </m:mrow>
   <m:mi>h</m:mi>
  </m:mfrac>
  <m:mo>−</m:mo><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mfrac>
   <m:mrow>
    <m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>h</m:mi>
   </m:mrow>
   <m:mi>h</m:mi>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></problem>
        <solution id="fs-id2167167">
          <para id="fs-id2167170"> <m:math>
 <m:mrow>
  <m:mtable>
   <m:mtr rowalign="center">
    <m:mtd columnalign="right" rowalign="center">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo stretchy="false">)</m:mo><m:mo>−</m:mo><m:mi>cos</m:mi><m:mi>x</m:mi>
       </m:mrow>
       <m:mi>h</m:mi>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd rowalign="center"/>
   </m:mtr>
   <m:mtr rowalign="center">
    <m:mtd columnalign="right" rowalign="center">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mi>cos</m:mi><m:mi>x</m:mi><m:mi>cosh</m:mi><m:mo>−</m:mo><m:mi>sin</m:mi><m:mi>x</m:mi><m:mi>sinh</m:mi><m:mo>−</m:mo><m:mi>cos</m:mi><m:mi>x</m:mi>
       </m:mrow>
       <m:mi>h</m:mi>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd rowalign="center"/>
   </m:mtr>
   <m:mtr rowalign="center">
    <m:mtd columnalign="right" rowalign="center">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mi>cos</m:mi><m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>cosh</m:mi><m:mo>−</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>−</m:mo><m:mi>sin</m:mi><m:mi>x</m:mi><m:mi>sinh</m:mi>
       </m:mrow>
       <m:mi>h</m:mi>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left" rowalign="center">
     <m:mrow>
      <m:mi>cos</m:mi><m:mi>x</m:mi><m:mfrac>
       <m:mrow>
        <m:mi>cos</m:mi><m:mi>h</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
       </m:mrow>
       <m:mi>h</m:mi>
      </m:mfrac>
      <m:mo>−</m:mo><m:mi>sin</m:mi><m:mi>x</m:mi><m:mfrac>
       <m:mrow>
        <m:mi>sin</m:mi><m:mi>h</m:mi>
       </m:mrow>
       <m:mi>h</m:mi>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></solution></exercise>


