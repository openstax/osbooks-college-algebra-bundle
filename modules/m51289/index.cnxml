</m:math>
</para></problem></exercise>
      <exercise id="fs-id2613308"><problem id="fs-id2613309"><para id="fs-id2613310"> <m:math>
 <m:mrow>
  <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>345°</m:mn><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math>
</para></problem>
      <solution id="fs-id2214309">
        <para id="fs-id1676814"> <m:math>
          <m:mrow>
            <m:mfrac>
              <m:mrow>
                <m:mn>1</m:mn><m:mo>+</m:mo><m:msqrt>
                  <m:mn>3</m:mn>
                </m:msqrt>

              </m:mrow>
              <m:mrow>
                <m:mn>2</m:mn><m:msqrt>
                  <m:mn>2</m:mn>
                </m:msqrt>

              </m:mrow>
            </m:mfrac>
            <m:mo>,</m:mo>
          </m:mrow>
        </m:math> or 0.9659</para></solution></exercise>
