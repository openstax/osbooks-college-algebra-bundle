      <exercise id="fs-id1681120"><problem id="fs-id2255437"><para id="fs-id2255438"> <m:math>
        <m:mrow>
          <m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mi>π</m:mi>
                <m:mn>4</m:mn>
              </m:mfrac>
              <m:mo>+</m:mo><m:mi>x</m:mi>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math>
      </para></problem>
      <solution id="fs-id1752326">
        <para id="fs-id1752329"> <m:math>
          <m:mrow>
            <m:mfrac>
              <m:mrow>
                <m:mi>sin</m:mi><m:mi>x</m:mi>
              </m:mrow>
              <m:mrow>
                <m:msqrt>
                  <m:mn>2</m:mn>
                </m:msqrt>

              </m:mrow>
            </m:mfrac>
            <m:mo>+</m:mo><m:mfrac>
              <m:mrow>
                <m:mi>cos</m:mi><m:mi>x</m:mi>
              </m:mrow>
              <m:mrow>
                <m:msqrt>
                  <m:mn>2</m:mn>
                </m:msqrt>

              </m:mrow>
            </m:mfrac>

          </m:mrow>
        </m:math>
        </para>

        <media id="fs-id2199923" alt="Graph of y = sin(x) / rad2 + cos(x) / rad2 - it looks like the sin curve shifted by pi/4." display="block">
          <image mime-type="image/jpg" src="../../media/CNX_Precalc_Figure_07_02_207.jpg"/>
        </media>


</solution></exercise><exercise id="fs-id2199934"><problem id="fs-id2199935"><para id="fs-id1672664"> <m:math>
        <m:mrow>
          <m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
