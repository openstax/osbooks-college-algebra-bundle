     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>
        <m:mo>+</m:mo><m:mn>1</m:mn>
       </m:mrow>
       <m:mrow>
        <m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>
        <m:mo>−</m:mo><m:mn>1</m:mn>
