      <m:mo>−</m:mo><m:mfrac>
       <m:mrow>
        <m:mi>sin</m:mi><m:mi>a</m:mi><m:mi>sin</m:mi><m:mi>b</m:mi>
       </m:mrow>
       <m:mrow>
        <m:mi>cos</m:mi><m:mi>a</m:mi><m:mi>cos</m:mi><m:mi>b</m:mi>
       </m:mrow>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left" rowalign="center">
     <m:mrow>
      <m:mn>1</m:mn><m:mo>−</m:mo><m:mi>tan</m:mi><m:mi>a</m:mi><m:mi>tan</m:mi><m:mi>b</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></solution></exercise>
      <exercise id="fs-id2467728" class="material-set-2"><problem id="fs-id2467729"><para id="fs-id2467730"> <m:math>
        <m:mrow>
