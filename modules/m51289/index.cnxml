    </m:mtd>
   </m:mtr>
   <m:mtr rowalign="center">
    <m:mtd rowalign="center"/>
    <m:mtd rowalign="center">
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd columnalign="left" rowalign="center">
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:mn>1</m:mn><m:mo>+</m:mo><m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>

       </m:mrow>
       <m:mrow>
        <m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>

       </m:mrow>
      </m:mfrac>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mfrac>
         <m:mrow>
          <m:msqrt>
           <m:mn>3</m:mn>
          </m:msqrt>

         </m:mrow>
         <m:mrow>
          <m:msqrt>
           <m:mn>3</m:mn>
