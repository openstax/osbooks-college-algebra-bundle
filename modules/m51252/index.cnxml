 <m:mi>x</m:mi>
</m:math> </entry>
<entry align="center"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
 </m:mrow>
</m:math> </entry>
<entry align="center"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mi>x</m:mi><m:mo>,</m:mo><m:mi>y</m:mi>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math> </entry>
</row>


<row>
<entry align="center"> <m:math>
 <m:mrow>
  <m:mn>−5</m:mn>
 </m:mrow>
</m:math> </entry>
<entry align="center"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mn>−5</m:mn>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mn>2</m:mn><m:mo>=</m:mo><m:mn>7</m:mn>
 </m:mrow>
