<section id="fs-id1165137659195" class="key-equations"><title>Key Equations</title><table id="eip-id1362369" summary=".." class="unnumbered unstyled"><label/><tgroup cols="1">
<colspec colnum="1" colname="c1"/>
<tbody>
<row>
<entry valign="middle" align="left">Rational Function</entry>
<entry align="center"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mi>P</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
   </m:mrow>
   <m:mrow>
    <m:mi>Q</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
   </m:mrow>
  </m:mfrac>
  <m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:msub>
     <m:mi>a</m:mi>
     <m:mi>p</m:mi>
    </m:msub>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mi>p</m:mi>
    </m:msup>
    <m:mo>+</m:mo><m:msub>
     <m:mi>a</m:mi>
     <m:mrow>
      <m:mi>p</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msub>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mrow>
      <m:mi>p</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msup>
    <m:mo>+</m:mo><m:mn>...</m:mn><m:mo>+</m:mo><m:msub>
     <m:mi>a</m:mi>
     <m:mn>1</m:mn>
    </m:msub>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:msub>
     <m:mi>a</m:mi>
     <m:mn>0</m:mn>
    </m:msub>

   </m:mrow>
   <m:mrow>
    <m:msub>
     <m:mi>b</m:mi>
     <m:mi>q</m:mi>
    </m:msub>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mi>q</m:mi>
    </m:msup>
    <m:mo>+</m:mo><m:msub>
     <m:mi>b</m:mi>
     <m:mrow>
      <m:mi>q</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msub>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mrow>
      <m:mi>q</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msup>
    <m:mo>+</m:mo><m:mn>...</m:mn><m:mo>+</m:mo><m:msub>
     <m:mi>b</m:mi>
     <m:mn>1</m:mn>
    </m:msub>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:msub>
