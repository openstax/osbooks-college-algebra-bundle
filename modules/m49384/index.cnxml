 <m:mrow>
  <m:mi>cot</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>=</m:mo><m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mi>π</m:mi>
     <m:mn>2</m:mn>
    </m:mfrac>
    <m:mo>−</m:mo><m:mi>t</m:mi>
