    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>s</m:mi><m:mi>i</m:mi><m:mi>n</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
