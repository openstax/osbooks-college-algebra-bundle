</m:math> </para></problem>
<solution id="fs-id1165137767858"><para id="fs-id1165137767859"> <m:math>
 <m:mrow>
  <m:mo>−</m:mo><m:mfrac>
   <m:mn>5</m:mn>
   <m:mrow>
    <m:mn>4</m:mn><m:mi>x</m:mi>
   </m:mrow>
  </m:mfrac>
  <m:mo>−</m:mo><m:mfrac>
   <m:mn>5</m:mn>
   <m:mrow>
    <m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn>
     </m:mrow>
    <m:mo>)</m:mo></m:mrow>
   </m:mrow>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>11</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>
     </m:mrow>
    <m:mo>)</m:mo></m:mrow>
   </m:mrow>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mn>5</m:mn>
   <m:mrow>
    <m:mn>4</m:mn><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>
     </m:mrow>
    <m:mo>)</m:mo></m:mrow>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para></solution>
