<problem id="fs-id1165135697780"><para id="fs-id1165135697781"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>2</m:mn><m:mi>x</m:mi>
   </m:mrow>
