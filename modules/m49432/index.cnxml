   </m:mrow>
  </m:mfrac>
  <m:mo>−</m:mo><m:mfrac>
   <m:mrow>
    <m:mi>x</m:mi><m:mn>−5</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msup>
