   </m:mrow>
  </m:mfrac>
  <m:mo>−</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>1</m:mn><m:mn>−2</m:mn><m:mi>x</m:mi>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
