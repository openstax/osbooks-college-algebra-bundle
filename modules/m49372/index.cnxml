        <m:mfrac>
 <m:mrow>
          <m:mo>−</m:mo><m:mi>π</m:mi></m:mrow>
         <m:mn>3</m:mn>
        </m:mfrac>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow></m:mrow></m:math>
   </para>
  </problem>
 </exercise>
 <exercise id="fs-id1165135149256">
  <problem id="fs-id1165135149258">
   <para id="fs-id1165135149260"> <m:math>
<m:mrow>
     <m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
  <m:mrow>
       <m:mfrac>
<m:mrow>
         <m:mn>7</m:mn><m:mi>π</m:mi></m:mrow>
        <m:mn>4</m:mn>
       </m:mfrac>
      </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
        <m:mfrac>
 <m:mrow>
          <m:mo>−</m:mo><m:mn>2</m:mn><m:mi>π</m:mi></m:mrow>
         <m:mn>3</m:mn>
        </m:mfrac>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow></m:mrow></m:math>
   </para>
  </problem>
  <solution id="fs-id1165137925605"><para id="fs-id1165137925606"> <m:math>
   <m:mrow>
    <m:mfrac>
 <m:mrow>
      <m:msqrt>
       <m:mn>2</m:mn>
      </m:msqrt>
     </m:mrow>
     <m:mn>4</m:mn>
    </m:mfrac></m:mrow>
  </m:math>
  </para></solution>
 </exercise>
 <exercise id="fs-id1165137704516">
