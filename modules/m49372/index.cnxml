<image mime-type="image/jpg" src="../../media/CNX_Precalc_Figure_05_02_204-45e8.jpg"/>
</media>
</problem>
<solution id="fs-id1165137888941">
 <para id="fs-id1165137888943"> <m:math>
 <m:mrow>
  <m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
   <m:mrow>
    <m:msqrt>
     <m:mn>2</m:mn>
