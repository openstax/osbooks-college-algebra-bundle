</exercise>

<exercise id="fs-id1165135685159">
<problem id="fs-id1165135685161">
<para id="fs-id1165135685163"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mrow><m:mo>|</m:mo> <m:mrow>
     <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
    </m:mrow> <m:mo>|</m:mo></m:mrow>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>−</m:mo><m:mn>2</m:mn><m:mi>x</m:mi>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165135685230">
<para id="fs-id1165135685233">Discontinuous at <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
 </m:mrow>
</m:math> and <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>2</m:mn>
 </m:mrow>
</m:math> </para>
</solution>
</exercise>

<exercise id="fs-id1165135685265">
<problem id="fs-id1165135685267">
<para id="fs-id1165135685269"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
