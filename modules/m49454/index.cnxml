<exercise id="fs-id1165135693041"><problem id="fs-id1165135693043"><para id="fs-id1165135693044"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>ln</m:mi><m:mspace width="0.8em"/><m:msup>
