<exercise id="fs-id1165135692926">
<problem id="fs-id1165135692929">
<para id="fs-id1165135692931"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mfrac>
   <m:mn>5</m:mn>
