<exercise id="fs-id1165135690656" class="material-set-2">
<problem id="fs-id1165135690658">
<para id="fs-id1165135690660"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>3</m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>&lt;</m:mo><m:mn>1</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mo>−</m:mo><m:msup>
        <m:mi>x</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>></m:mo><m:mn>1</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable><m:mtext>    </m:mtext><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
  </m:mrow> </m:mrow>
 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id1165135683053">
<para id="fs-id1165135683055"> <m:math>
 <m:mrow>
  <m:munder>
   <m:mrow>
    <m:mi>lim</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:msup>
     <m:mn>1</m:mn>
     <m:mo>−</m:mo>
    </m:msup>

   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>5</m:mn><m:mo>≠</m:mo><m:munder>
   <m:mrow>
    <m:mi>lim</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:msup>
     <m:mn>1</m:mn>
     <m:mo>+</m:mo>
    </m:msup>

   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>1</m:mn>
 </m:mrow>
</m:math>. Thus <m:math>
 <m:mrow>
  <m:munder>
   <m:mrow>
    <m:mi>lim</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mn>1</m:mn>
   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math> does not exist.</para>
</solution>
</exercise>

