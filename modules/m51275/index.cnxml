    <entry/>
  </row>
  <row>
    <entry> <m:math><m:mo>-</m:mo><m:mn>2</m:mn><m:msup><m:mi>x</m:mi><m:mrow><m:mn>2</m:mn><m:mo>/</m:mo><m:mn>3</m:mn></m:mrow></m:msup><m:mo>-</m:mo><m:mn>5</m:mn><m:mi>x</m:mi></m:math> </entry>
    <entry/>
    <entry/>
    <entry/>
