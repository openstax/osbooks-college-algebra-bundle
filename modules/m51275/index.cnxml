<thead>
<row>
<entry align="center"> <m:math>
 <m:mi>x</m:mi>
</m:math>
</entry>
<entry align="center"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
