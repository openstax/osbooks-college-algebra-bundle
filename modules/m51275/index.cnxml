<exercise id="fs-id1165137678592"><problem id="fs-id1165137678593"><para id="fs-id1165137678594"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:msup>
   <m:mi>x</m:mi>
   <m:mn>4</m:mn>
  </m:msup>

 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165137643430"><para id="fs-id1165137643431">As <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mo>−</m:mo><m:mi>∞</m:mi></m:mrow></m:math>, <m:math><m:mrow><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mo>−</m:mo><m:mi>∞</m:mi><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext>as</m:mtext><m:mspace width="0.8em"/><m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mi>∞</m:mi><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mo>−</m:mo><m:mi>∞</m:mi>
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1165135557775"><problem id="fs-id1165135457738"><para id="fs-id1165135457739"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:msup>
