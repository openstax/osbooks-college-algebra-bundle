   </m:mrow>
   <m:mn>2</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo>
 </m:mrow>
</m:math> <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>1</m:mn>
