      <m:mrow>
        <m:mi>b</m:mi><m:mo>=</m:mo><m:mn>3.5</m:mn><m:mo>,</m:mo></m:mrow></m:math> <m:math><m:mrow><m:mi>c</m:mi><m:mo>=</m:mo><m:mn>5.3</m:mn><m:mo>,</m:mo></m:mrow></m:math> <m:math><m:mrow><m:mi>γ</m:mi><m:mo>=</m:mo><m:mspace width="0.3em"/>
