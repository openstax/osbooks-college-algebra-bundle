      <m:mrow>
        <m:mi>a</m:mi><m:mo>=</m:mo><m:mn>4</m:mn><m:mo>,</m:mo></m:mrow></m:math> <m:math><m:mrow><m:mi>α</m:mi><m:mo>=</m:mo><m:mspace width="0.3em"/>
          <m:mrow>
            <m:mn>60°</m:mn></m:mrow>
        <m:mo>,</m:mo></m:mrow></m:math> <m:math><m:mrow><m:mi>β</m:mi><m:mo>=</m:mo><m:mn>100°</m:mn></m:mrow>
    </m:math>
    </para></problem>
<solution id="fs-id1165137911000">
