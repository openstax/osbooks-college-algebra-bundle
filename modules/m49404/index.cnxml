  <para id="fs-id1165133199364"> <m:math>
    <m:mrow>
      <m:mn>12.3</m:mn></m:mrow>
  </m:math>
  </para>
</solution>
</exercise><exercise id="fs-id1165137765789"><problem id="fs-id1165137765791">
