        <m:mi>a</m:mi><m:mo>=</m:mo><m:mn>7.2</m:mn><m:mo>,</m:mo><m:mi>b</m:mi><m:mo>=</m:mo><m:mn>4.5</m:mn><m:mo>,</m:mo><m:mi>γ</m:mi><m:mo>=</m:mo>
          <m:mrow>
            <m:mn>43°</m:mn></m:mrow>
