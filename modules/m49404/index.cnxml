      <m:mrow>
        <m:mi>b</m:mi><m:mo>=</m:mo><m:mn>10</m:mn><m:mo>,</m:mo></m:mrow></m:math> <m:math><m:mrow><m:mi>β</m:mi><m:mo>=</m:mo><m:mn>95°</m:mn><m:mo>,</m:mo><m:mi>γ</m:mi><m:mo>=</m:mo><m:mspace width="0.3em"/>
