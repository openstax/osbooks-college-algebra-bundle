<m:mi>A</m:mi><m:mo>=</m:mo><m:mn>132°</m:mn><m:mo>,</m:mo><m:mi>C</m:mi><m:mo>=</m:mo><m:mn>23°</m:mn><m:mo>,</m:mo><m:mi>b</m:mi><m:mo>=</m:mo><m:mn>10.</m:mn></m:mrow>
