          <m:mrow>
            <m:mn>35°</m:mn></m:mrow>
      </m:mrow>
    </m:math>
    </para></problem>
<solution id="fs-id1165135697825">
  <para id="fs-id1165135697827"> <m:math>
    <m:mrow>
      <m:mn>8.6</m:mn></m:mrow>
  </m:math>
  </para>
</solution>
</exercise>
