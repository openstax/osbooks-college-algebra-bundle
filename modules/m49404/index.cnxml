</exercise>
<exercise id="fs-id1165135521227">
  <problem id="fs-id1165135521229">
    <para id="fs-id1165135521231"> <m:math>
      <m:mrow>
        <m:mi>a</m:mi><m:mo>=</m:mo><m:mn>12</m:mn><m:mo>,</m:mo></m:mrow></m:math> <m:math><m:mrow><m:mi>c</m:mi><m:mo>=</m:mo><m:mn>17</m:mn><m:mo>,</m:mo></m:mrow></m:math> <m:math><m:mrow><m:mi>α</m:mi><m:mo>=</m:mo><m:mspace width="0.3em"/>
          <m:mrow>
            <m:mn>35°</m:mn></m:mrow>
      </m:mrow>
    </m:math>
    </para></problem>
<solution id="fs-id1165134224075">
  <para id="fs-id1165135518098">two triangles, <m:math>
    <m:mrow>
      <m:mi>γ</m:mi><m:mo>≈</m:mo><m:mn>54.3°</m:mn><m:mo>,</m:mo><m:mi>β</m:mi><m:mo>≈</m:mo><m:mn>90.7°</m:mn><m:mo>,</m:mo><m:mi>b</m:mi><m:mo>≈</m:mo><m:mn>20.9</m:mn></m:mrow>
  </m:math> or <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>γ</m:mi>
   <m:mo>′</m:mo>
  </m:msup>
  <m:mo>≈</m:mo><m:mn>125.7°</m:mn><m:mo>,</m:mo><m:msup>
   <m:mi>β</m:mi>
   <m:mo>′</m:mo>
  </m:msup>
  <m:mo>≈</m:mo><m:mn>19.3°</m:mn><m:mo>,</m:mo><m:msup>
   <m:mi>b</m:mi>
   <m:mo>′</m:mo>
  </m:msup>
  <m:mo>≈</m:mo><m:mn>6.9</m:mn>
 </m:mrow>
</m:math>   </para></solution></exercise>
<exercise id="fs-id1165134073101">
  <problem id="fs-id1165134073103">
