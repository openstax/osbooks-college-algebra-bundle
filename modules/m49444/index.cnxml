  </m:msub>
  <m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>3</m:mn><m:mo>⋅</m:mo><m:mi>n</m:mi><m:mo>!</m:mo>
   </m:mrow>
