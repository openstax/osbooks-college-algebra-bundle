<para id="fs-id1165137772281"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mn>1</m:mn>
  </m:msub>
  <m:mo>=</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:msub>
   <m:mrow>
    <m:mspace width="0.8em"/><m:mtext>a</m:mtext>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msub>
  <m:mo>=</m:mo><m:mn>10</m:mn><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:msub>
       <m:mi>a</m:mi>
       <m:mrow>
        <m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
       </m:mrow>
      </m:msub>
      <m:mo>+</m:mo><m:mn>2</m:mn>
     </m:mrow>
    <m:mo>)</m:mo></m:mrow>
   </m:mrow>
   <m:mrow>
    <m:msub>
     <m:mi>a</m:mi>
     <m:mrow>
      <m:mi>n</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
     </m:mrow>
    </m:msub>

   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para>
