    <m:mn>2</m:mn><m:mi>n</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>n</m:mi>
     <m:mn>3</m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para>
</problem>
</exercise>

