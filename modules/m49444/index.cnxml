   <m:mrow>
    <m:mn>11</m:mn>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para>
</solution>
</exercise>

<exercise id="fs-id1165135593352">
<problem id="fs-id1165137693539">
<para id="fs-id1165137693541"> <m:math>
 <m:mrow>
  <m:msub>
