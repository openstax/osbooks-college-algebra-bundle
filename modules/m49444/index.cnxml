<para id="fs-id1165135404267">For the following exercises, write the first eight terms of the piecewise sequence.</para>

<exercise id="fs-id1165135404270" class="material-set-2">
<problem id="fs-id1165135404272">
<para id="fs-id1165135404274"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:msup>
        <m:mrow>
         <m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo>
        </m:mrow>
        <m:mi>n</m:mi>
       </m:msup>
       <m:mo>−</m:mo><m:mn>2</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext>is even</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:msup>
        <m:mrow>
         <m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo>
        </m:mrow>
        <m:mrow>
         <m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
        </m:mrow>
       </m:msup>

      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext>is odd</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> </m:mrow>
 </m:mrow>
</m:math> </para>
</problem>
</exercise>

<exercise id="fs-id1165137626830" class="material-set-2">
<problem id="fs-id1165137626832">
<para id="fs-id1165137626834"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mi>n</m:mi>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:mn>2</m:mn><m:mi>n</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
        </m:mrow>
       </m:mfrac>

      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext/><m:mo>≤</m:mo><m:mtext>5</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:msup>
        <m:mi>n</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:mn>5</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext>>5</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> </m:mrow>
 </m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165135570066">
<para id="fs-id1165135570068"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mn>1</m:mn>
   <m:mn>3</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mfrac>
   <m:mn>4</m:mn>
   <m:mn>5</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mfrac>
   <m:mn>9</m:mn>
   <m:mn>7</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mfrac>
   <m:mrow>
    <m:mn>16</m:mn>
   </m:mrow>
   <m:mn>9</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mfrac>
   <m:mrow>
    <m:mn>25</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>11</m:mn>
   </m:mrow>
  </m:mfrac>
  <m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mn>31</m:mn><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mn>44</m:mn><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mn>59</m:mn>
 </m:mrow>
</m:math> </para>
</solution>
</exercise>

<exercise id="fs-id1165135205807" class="material-set-2">
<problem id="fs-id1165135205809">
<para id="fs-id1165135205811"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:msup>
        <m:mrow>
         <m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mi>n</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
        </m:mrow>
        <m:mn>2</m:mn>
       </m:msup>

      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext>is divisible by 4</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mn>2</m:mn>
        <m:mi>n</m:mi>
       </m:mfrac>

      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext>is not divisible by 4</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> </m:mrow>
 </m:mrow>
</m:math> </para>
</problem>
</exercise>

<exercise id="fs-id1165135245783" class="material-set-2">
<problem id="fs-id1165135199590">
<para id="fs-id1165135199592"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mo>−</m:mo><m:mn>0.6</m:mn><m:mo>⋅</m:mo><m:msup>
        <m:mn>5</m:mn>
        <m:mrow>
         <m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
        </m:mrow>
       </m:msup>

      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext>is prime or 1</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>2.5</m:mn><m:mo>⋅</m:mo><m:msup>
        <m:mrow>
         <m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo>
        </m:mrow>
        <m:mrow>
         <m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
        </m:mrow>
       </m:msup>

      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext>is composite</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> </m:mrow>
 </m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165137476952">
<para id="fs-id1165137476954"> <m:math>
 <m:mrow>
  <m:mo>−</m:mo><m:mn>0.6</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>15</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>20</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>375</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>80</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>9375</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>320</m:mn>
 </m:mrow>
</m:math> </para>
</solution>
</exercise>

<exercise id="fs-id1165137629052" class="material-set-2">
<problem id="fs-id1165137629054">
<para id="fs-id1165137629056"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>4</m:mn><m:mo stretchy="false">(</m:mo><m:msup>
        <m:mi>n</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext/><m:mo>≤</m:mo><m:mn>3</m:mn><m:mspace width="0.8em"/><m:mtext>or </m:mtext><m:mi>n</m:mi><m:mspace width="0.8em"/><m:mtext>> 6</m:mtext>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mi>n</m:mi>
          <m:mn>2</m:mn>
         </m:msup>
         <m:mo>−</m:mo><m:mn>2</m:mn>
        </m:mrow>
        <m:mn>4</m:mn>
       </m:mfrac>

      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mtext>if </m:mtext><m:mn>3</m:mn><m:mo>&lt;</m:mo><m:mi>n</m:mi><m:mo>≤</m:mo><m:mn>6</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> </m:mrow>
 </m:mrow>
</m:math> </para>
</problem>
</exercise>

<para id="fs-id1165135194285">For the following exercises, write an explicit formula for each sequence.</para>

<exercise id="fs-id1165137851677">
<problem id="fs-id1165137851679">
<para id="fs-id1165137851681"> <m:math>
 <m:mrow>
  <m:mn>4</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mn>7</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mn>12</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mn>19</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mn>28</m:mn><m:mo>,</m:mo><m:mo>…</m:mo>
 </m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165135253859">
<para id="fs-id1165135253861"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:msup>
   <m:mi>n</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>3</m:mn>
 </m:mrow>
</m:math> </para>
</solution>
</exercise>

<exercise id="fs-id1165137838727">
<problem id="fs-id1165137838729">
<para id="fs-id1165137838732"> <m:math>
 <m:mrow>
  <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>10</m:mn><m:mo>,</m:mo><m:mn>14</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>34</m:mn><m:mo>,</m:mo><m:mo>…</m:mo>
 </m:mrow>
</m:math> </para>
</problem>
</exercise>

<exercise id="fs-id1165137849326">
<problem id="fs-id1165137849329">
<para id="fs-id1165137849331"> <m:math>
 <m:mrow>
  <m:mn>1</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mfrac>
   <m:mn>4</m:mn>
   <m:mn>3</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>16</m:mn>
   </m:mrow>
   <m:mn>5</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mo>…</m:mo>
 </m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165135541602">
<para id="fs-id1165135541604"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mn>2</m:mn>
     <m:mi>n</m:mi>
    </m:msup>

   </m:mrow>
   <m:mrow>
    <m:mn>2</m:mn><m:mi>n</m:mi>
   </m:mrow>
  </m:mfrac>
  <m:mspace width="0.8em"/><m:mtext>or </m:mtext><m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mn>2</m:mn>
     <m:mrow>
      <m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msup>

   </m:mrow>
   <m:mi>n</m:mi>
  </m:mfrac>

 </m:mrow>
</m:math> </para>
</solution>
</exercise>

