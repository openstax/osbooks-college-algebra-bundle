<section id="fs-id1800200" class="key-equations">
 <title>Key Equations</title>
 <table id="fs-id1800206" summary=".." class="unnumbered unstyled"><label/>
<tgroup cols="2"><colspec colnum="1" colname="c1"/>
   <colspec colnum="2" colname="c2"/>
