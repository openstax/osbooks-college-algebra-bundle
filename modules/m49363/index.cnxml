<m:mtext>ln</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>w</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>n</m:mi>
</m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165135185210">
<para id="fs-id1165135185212"> <m:math>
<m:mrow>
<m:msup>
<m:mi>e</m:mi>
<m:mi>n</m:mi>
</m:msup>
<m:mo>=</m:mo><m:mi>w</m:mi>
</m:mrow>
</m:math> </para>
</solution>
</exercise>

