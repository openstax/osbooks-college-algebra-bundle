<exercise id="fs-id1165137851686">
<problem id="fs-id1165137851688">
<para id="fs-id1165137851690"> <m:math>
<m:mrow>
<m:mi>k</m:mi><m:mrow><m:mo>(</m:mo>
<m:mi>x</m:mi>
<m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>5</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
