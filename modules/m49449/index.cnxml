<exercise id="fs-id1165137442493">
<problem id="fs-id1165137442495">
<para id="fs-id1165137724327"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mrow>
    <m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo>
   </m:mrow>
   <m:mn>3</m:mn>
  </m:msup>

 </m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165137831242">
<para id="fs-id1165137769967"> <m:math>
 <m:mrow>
  <m:mn>64</m:mn><m:msup>
   <m:mi>a</m:mi>
   <m:mn>3</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mn>48</m:mn><m:msup>
   <m:mi>a</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>12</m:mn><m:mi>a</m:mi><m:msup>
   <m:mi>b</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:msup>
   <m:mi>b</m:mi>
   <m:mn>3</m:mn>
  </m:msup>

 </m:mrow>
</m:math> </para>
</solution>
</exercise>

