<exercise id="fs-id1165135526126">
<problem id="fs-id1165135526128">
<para id="fs-id1165135526130"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mrow>
    <m:mo stretchy="false">(</m:mo><m:msup>
     <m:mi>x</m:mi>
     <m:mrow>
      <m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msup>
    <m:mo>+</m:mo><m:mn>2</m:mn><m:msup>
     <m:mi>y</m:mi>
     <m:mrow>
      <m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msup>
    <m:mo stretchy="false">)</m:mo>
   </m:mrow>
   <m:mn>4</m:mn>
  </m:msup>

 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165137462052">
<para id="fs-id1165137805817"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mn>1</m:mn>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>4</m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mn>8</m:mn>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>3</m:mn>
    </m:msup>
    <m:mi>y</m:mi>
   </m:mrow>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>24</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:msup>
     <m:mi>y</m:mi>
     <m:mn>2</m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>32</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:msup>
     <m:mi>y</m:mi>
     <m:mn>3</m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>16</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mi>y</m:mi>
     <m:mn>4</m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para>
</solution>
