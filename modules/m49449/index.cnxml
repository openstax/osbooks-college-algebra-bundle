<solution id="fs-id1165137527392">
<para id="fs-id1165137535275"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>a</m:mi>
   <m:mrow>
    <m:mn>17</m:mn>
   </m:mrow>
  </m:msup>
  <m:mo>+</m:mo><m:mn>17</m:mn><m:msup>
   <m:mi>a</m:mi>
   <m:mrow>
    <m:mn>16</m:mn>
   </m:mrow>
  </m:msup>
  <m:mi>b</m:mi><m:mo>+</m:mo><m:mn>136</m:mn><m:msup>
   <m:mi>a</m:mi>
   <m:mrow>
    <m:mn>15</m:mn>
   </m:mrow>
  </m:msup>
  <m:msup>
   <m:mi>b</m:mi>
   <m:mn>2</m:mn>
  </m:msup>

 </m:mrow>
</m:math> </para>
</solution>
</exercise>

