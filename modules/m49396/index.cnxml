        </m:mrow>
      </m:math>
      </para></problem>
      <solution id="fs-id1541636">
        <para id="fs-id1541638"> <m:math>
          <m:mrow>
            <m:mn>2</m:mn><m:mo>+</m:mo><m:msqrt>
              <m:mn>3</m:mn>
            </m:msqrt>

          </m:mrow>
        </m:math>
        </para>
      </solution></exercise>
      <exercise id="fs-id1562395"><problem id="fs-id1562396"><para id="fs-id1562397"> <m:math>
        <m:mrow>
          <m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mo>−</m:mo><m:mfrac>
                <m:mrow>
                  <m:mn>3</m:mn><m:mi>π</m:mi>
                </m:mrow>
