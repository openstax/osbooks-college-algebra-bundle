        </m:mrow>
      </m:math>
      </para></problem></exercise>
      <exercise id="fs-id1919766" class="material-set-2"><problem id="fs-id1919767"><para id="fs-id1919768"> <m:math>
        <m:mrow>
          <m:mfrac>
            <m:mrow>
              <m:mn>1</m:mn><m:mo>+</m:mo><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mn>2</m:mn><m:mi>θ</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow>
            </m:mrow>
<m:mrow>
              <m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mn>2</m:mn><m:mi>θ</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow>
            </m:mrow>
          </m:mfrac>


          <m:msup>
            <m:mrow>
              <m:mi>tan</m:mi>
            </m:mrow>
            <m:mn>2</m:mn>
          </m:msup>
          <m:mi>θ</m:mi><m:mo>=</m:mo><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
        </m:mrow>
      </m:math>
      </para></problem>
        <solution id="fs-id2259856">
          <para id="fs-id2259858"> <m:math>
            <m:mtable columnalign="left">
              <m:mtr>
                <m:mtd>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mrow>
                          <m:mn>2</m:mn><m:mi>θ</m:mi>
                        </m:mrow>
                        <m:mo>)</m:mo></m:mrow>
                    </m:mrow>
                    <m:mrow>
                      <m:mn>1</m:mn><m:mo>+</m:mo><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mrow>
                          <m:mn>2</m:mn><m:mi>θ</m:mi>
                        </m:mrow>
                        <m:mo>)</m:mo></m:mrow>
                    </m:mrow>
                  </m:mfrac>
                  <m:msup>
                    <m:mi>tan</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mi>θ</m:mi><m:mo>=</m:mo><m:mfrac>
                    <m:mrow>
                      <m:mn>2</m:mn><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mi>θ</m:mi>
                        <m:mo>)</m:mo></m:mrow><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                          <m:mi>θ</m:mi>
                          <m:mo>)</m:mo></m:mrow>
                    </m:mrow>
                    <m:mrow>
                      <m:mn>1</m:mn><m:mo>+</m:mo><m:msup>
                        <m:mrow>
                          <m:mi>cos</m:mi>
                        </m:mrow>
                        <m:mn>2</m:mn>
                      </m:msup>
                      <m:mi>θ</m:mi><m:mo>−</m:mo><m:msup>
                        <m:mrow>
                          <m:mi>sin</m:mi>
                        </m:mrow>
                        <m:mn>2</m:mn>
                      </m:msup>
                      <m:mi>θ</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:msup>
                    <m:mi>tan</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mi>θ</m:mi><m:mo>=</m:mo>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd>
                  <m:mfrac>
                    <m:mrow>
                      <m:mn>2</m:mn><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mi>θ</m:mi>
                        <m:mo>)</m:mo></m:mrow><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                          <m:mi>θ</m:mi>
                          <m:mo>)</m:mo></m:mrow>
                    </m:mrow>
                    <m:mrow>
                      <m:mn>2</m:mn><m:msup>
                        <m:mrow>
                          <m:mi>cos</m:mi>
                        </m:mrow>
                        <m:mn>2</m:mn>
                      </m:msup>
                      <m:mi>θ</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:msup>
                    <m:mi>tan</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mi>θ</m:mi><m:mo>=</m:mo><m:mfrac>
                    <m:mrow>
                      <m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mi>θ</m:mi>
                        <m:mo>)</m:mo></m:mrow>
                    </m:mrow>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:msup>
                    <m:mi>tan</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mi>θ</m:mi><m:mo>=</m:mo>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd>
                  <m:mi>tan</m:mi><m:mrow>
                    <m:mi>θ</m:mi>
                    </m:mrow><m:msup>
                      <m:mi>tan</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                  <m:mi>θ</m:mi><m:mo>=</m:mo><m:msup><m:mi>tan</m:mi><m:mn>3</m:mn></m:msup><m:mspace width="0.5em"/><m:mi>θ</m:mi>
                </m:mtd>
              </m:mtr>
            </m:mtable>

          </m:math>
          </para>
        </solution></exercise>


      <para id="fs-id2307988">For the following exercises, rewrite the expression with an exponent no higher than 1.</para>
      <exercise id="fs-id2307992"><problem id="fs-id2307993"><para id="fs-id2307994"> <m:math>
        <m:mrow>
          <m:msup>
            <m:mrow>
