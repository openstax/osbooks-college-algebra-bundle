      <exercise id="fs-id2617871" class="material-set-2"><problem id="fs-id2617872"><para id="fs-id2617873"> <m:math>
        <m:mrow>
          <m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>2</m:mn><m:mi>x</m:mi>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mo>−</m:mo><m:mi>x</m:mi>
              </m:mrow>
              <m:mo>)</m:mo></m:mrow><m:mspace width="0.5em"/><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mo>−</m:mo><m:mi>x</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math>
      </para></problem>
        <solution id="fs-id2038541">
          <para id="fs-id2038543"> <m:math>
            <m:mrow>
              <m:mo>−</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mo>−</m:mo><m:mi>x</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                  <m:mrow>
                    <m:mo>−</m:mo><m:mi>x</m:mi>
                  </m:mrow>
                  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>2</m:mn><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
                    <m:mi>x</m:mi>
                    <m:mo>)</m:mo></m:mrow><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                      <m:mi>x</m:mi>
                      <m:mo>)</m:mo></m:mrow><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mrow>
                          <m:mn>2</m:mn><m:mi>x</m:mi>
                        </m:mrow>
                        <m:mo>)</m:mo></m:mrow>
            </m:mrow>
          </m:math>
          </para></solution></exercise>
      <exercise id="fs-id2116113" class="material-set-2"><problem id="fs-id2116114"><para id="fs-id2116115"> <m:math>
        <m:mrow>
          <m:mi>cot</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>−</m:mo><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>cot</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>2</m:mn><m:mi>x</m:mi>
            </m:mrow>
