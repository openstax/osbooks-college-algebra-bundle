    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>          </m:mtext><m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>4</m:mn>
      </m:mfrac>
      <m:mo>+</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mspace width="0.5em"/><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>4</m:mn>
      </m:mfrac>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mfrac>
         <m:mrow>
          <m:mn>1</m:mn><m:mo>+</m:mo><m:mi>cos</m:mi><m:mn>2</m:mn><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:mfrac>

       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mtable>
       <m:mtr>
        <m:mtd>
         <m:mrow/>
        </m:mtd>
