      <para id="fs-id2079018">For the following exercises, find the exact value using half-angle formulas.</para>
      <exercise id="fs-id2079021"><problem id="fs-id2079022"><para id="fs-id2079023"> <m:math>
        <m:mrow>
          <m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mi>π</m:mi>
                <m:mn>8</m:mn>
              </m:mfrac>

            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math>
      </para></problem>
      <solution id="fs-id1267575">
        <para id="fs-id1267577"> <m:math>
          <m:mrow>
            <m:mfrac>
              <m:mrow>
                <m:msqrt>
                  <m:mrow>
                    <m:mn>2</m:mn><m:mo>−</m:mo><m:msqrt>
                      <m:mn>2</m:mn>
                    </m:msqrt>

                  </m:mrow>
                </m:msqrt>

              </m:mrow>
              <m:mn>2</m:mn>
            </m:mfrac>

          </m:mrow>
        </m:math>
        </para>
      </solution></exercise>
      <exercise id="fs-id1256220"><problem id="fs-id1256221"><para id="fs-id1256222"> <m:math>
        <m:mrow>
          <m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mo>−</m:mo><m:mfrac>
                <m:mrow>
