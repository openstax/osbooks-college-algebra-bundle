        </m:math>
        </para>
      </solution></exercise>
      <exercise id="fs-id1613024"><problem id="fs-id1613025"><para id="fs-id1613026"> <m:math>
        <m:mrow>
          <m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
                  <m:mn>7</m:mn><m:mi>π</m:mi>
                </m:mrow>
                <m:mn>8</m:mn>
              </m:mfrac>

            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math>
      </para></problem></exercise>
      <exercise id="fs-id1274464"><problem id="fs-id1274465"><para id="fs-id1274466"> <m:math>
        <m:mrow>
          <m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
