              <m:mrow>
                <m:mn>119</m:mn>
              </m:mrow>
              <m:mrow>
                <m:mn>169</m:mn>
              </m:mrow>
            </m:mfrac>
            <m:mo>,</m:mo><m:mo>–</m:mo><m:mfrac>
              <m:mrow>
