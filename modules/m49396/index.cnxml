        </m:mrow>
      </m:math> and <m:math>
        <m:mrow>
          <m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mi>α</m:mi>
                <m:mn>2</m:mn>
              </m:mfrac>

            </m:mrow>
            <m:mo>)</m:mo><m:mo>.</m:mo></m:mrow>
        </m:mrow>
      </m:math>
      </para></problem>
        <solution id="fs-id1740865">
          <para id="fs-id1740867"> <m:math>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
                  <m:mn>2</m:mn><m:msqrt>
                    <m:mrow>
                      <m:mn>13</m:mn>
                    </m:mrow>
                  </m:msqrt>

                </m:mrow>
                <m:mrow>
                  <m:mn>13</m:mn>
                </m:mrow>
              </m:mfrac>
              <m:mo>,</m:mo><m:mfrac>
                <m:mrow>
                  <m:mn>3</m:mn><m:msqrt>
                    <m:mrow>
                      <m:mn>13</m:mn>
                    </m:mrow>
                  </m:msqrt>

                </m:mrow>
                <m:mrow>
                  <m:mn>13</m:mn>
                </m:mrow>
              </m:mfrac>
              <m:mo>,</m:mo><m:mfrac>
                <m:mn>2</m:mn>
                <m:mn>3</m:mn>
              </m:mfrac>

            </m:mrow>
          </m:math>
          </para>
        </solution></exercise>

