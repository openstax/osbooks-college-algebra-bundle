      <exercise id="fs-id2077632"><problem id="fs-id2077633"><para id="fs-id2077634"> <m:math>
        <m:mrow>
          <m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
