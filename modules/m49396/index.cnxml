        </m:mrow>
      </m:math>
      </para></problem>
        <solution id="fs-id2296536">
          <para id="fs-id2296538"> <m:math>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
                  <m:mn>1</m:mn><m:mo>+</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>12</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
                </m:mrow>
                <m:mn>2</m:mn>
              </m:mfrac>

            </m:mrow>
          </m:math>
          </para>
        </solution></exercise>
      <exercise id="fs-id2148526"><problem id="fs-id2148527"><para id="fs-id2148528"> <m:math>
